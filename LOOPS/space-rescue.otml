                                </OTCompoundDoc>
                                <OTCompoundDoc name="Hit the wall">
                                  <bodyText>Hit the wall</bodyText>
                                </OTCompoundDoc>
                                <OTCompoundDoc name="Press Left the same number of times as Right">
                                  <bodyText>Press Left the same number of times as Right</bodyText>
                                </OTCompoundDoc>
                                <OTCompoundDoc name="The Rocket stopped by itself">
                                  <bodyText>The Rocket stopped by itself</bodyText>
                                </OTCompoundDoc>
                              </choices>
                            </OTChoice>
                          </input>
