                    <OTCompoundDoc id="fb916175-7d53-11de-86c4-3d721432285a" name="%fb916175-7d53-11de-86c4-3d721432285a" showEditBar="true">
                      <documentRefs>
                        <OTQuestion id="b141ff4a-7d58-11de-86c4-3d721432285a" name="Dots (step 9)">
                          <prompt>
                            <OTCompoundDoc>
                              <bodyText>In this game, the Rocket leaves a trail of dots behind.  What do the dots tell you?</bodyText>
                            </OTCompoundDoc>
                          </prompt>
                          <input>
                            <OTChoice name="Multiple choice" horizontal="false" alignContent="0">
                              <choices>
                                <OTCompoundDoc name="The Rocket has power">
                                  <bodyText>The Rocket has power</bodyText>
                                </OTCompoundDoc>
                                <OTCompoundDoc name="The larger the space between the dots, the faster the Rocket is going">
                                  <bodyText>The larger the space between the dots, the faster the Rocket is going</bodyText>
                                </OTCompoundDoc>
                                <OTCompoundDoc name="The Rocket is about to run out of fuel">
                                  <bodyText>The Rocket is about to run out of fuel</bodyText>
                                </OTCompoundDoc>
                                <OTCompoundDoc name="The rocket goes at a constant speed">
                                  <bodyText>The rocket goes at a constant speed</bodyText>
                                </OTCompoundDoc>
                              </choices>
                            </OTChoice>
                          </input>
                        </OTQuestion>
                        <OTQuestion id="546fe868-7de0-11de-a225-011ffa63f2e0" name="Projection">
                          <prompt>
                            <OTCompoundDoc>
                              <bodyText>If the Rocket or Rocket plus Shuttle is stopped and you fire once and then twice more in the same direction, waiting for recharge between firings, and you don't hit a wall, the Rocket will go</bodyText>
                            </OTCompoundDoc>
                          </prompt>
                          <input>
                            <OTChoice name="Multiple choice" horizontal="false" alignContent="0">
                              <choices>
                                <OTCompoundDoc name="The same speed as firing twice">
                                  <bodyText>The same speed as firing twice</bodyText>
                                </OTCompoundDoc>
                                <OTCompoundDoc name="Three times the speed of firing once">
                                  <bodyText>Three times the speed of firing once</bodyText>
                                </OTCompoundDoc>
                                <OTCompoundDoc name="Four times the speed of firing once">
                                  <bodyText>Four times the speed of firing once</bodyText>
                                </OTCompoundDoc>
                                <OTCompoundDoc name="The change in speed is not always the same">
                                  <bodyText>The change in speed is not always the same</bodyText>
                                </OTCompoundDoc>
                              </choices>
                            </OTChoice>
                          </input>
                        </OTQuestion>
                        <OTQuestion id="31463896-7de2-11de-a225-011ffa63f2e0" name="StoppingRule">
                          <prompt>
                            <OTCompoundDoc>
                              <bodyText>What is a good rule for stopping the Rocket or for stopping the Rocket plus Shuttle?</bodyText>
                            </OTCompoundDoc>
                          </prompt>
                          <input>
                            <OTChoice name="Multiple choice" horizontal="false" alignContent="0">
                              <choices>
                                <OTCompoundDoc name="Fire once in the opposite direction from the direction it is moving ">
                                  <bodyText>Fire once in the opposite direction from the direction it is moving</bodyText>
                                </OTCompoundDoc>
                                <OTCompoundDoc name="Hit the brakes">
                                  <bodyText>Hit the brakes</bodyText>
                                </OTCompoundDoc>
                                <OTCompoundDoc name="Keep firing the Rocket until it stops ">
                                  <bodyText>Keep firing the Rocket until it stops</bodyText>
                                </OTCompoundDoc>
                                <OTCompoundDoc name="Fire the same number of  times in the opposite direction as you did to get it going">
                                  <bodyText>Fire the same number of  times in the opposite direction as you did to get it going</bodyText>
                                </OTCompoundDoc>
                              </choices>
                            </OTChoice>
                          </input>
                        </OTQuestion>
                        <OTQuestion id="b9df17fc-7de2-11de-a225-011ffa63f2e0" name="Question">
                          <prompt>
                            <OTCompoundDoc>
                              <bodyText>What is a good plan to get a high score?</bodyText>
                            </OTCompoundDoc>
                          </prompt>
                          <input>
                            <OTChoice name="Multiple choice" horizontal="false" alignContent="0">
                              <choices>
                                <OTCompoundDoc name="Always try to go as fast as possible ">
                                  <bodyText>Always try to go as fast as possible</bodyText>
                                </OTCompoundDoc>
                                <OTCompoundDoc name="Go slowly, because it is too hard to stop ">
                                  <bodyText>Go slowly, because it is too hard to stop</bodyText>
                                </OTCompoundDoc>
                                <OTCompoundDoc name="Just go as fast as you can being sure to leave time to stop ">
                                  <bodyText>Just go as fast as you can being sure to leave time to stop</bodyText>
                                </OTCompoundDoc>
                                <OTCompoundDoc name="Just avoid crashing into walls">
                                  <bodyText>Just avoid crashing into walls</bodyText>
                                </OTCompoundDoc>
                              </choices>
                            </OTChoice>
                          </input>
                        </OTQuestion>
