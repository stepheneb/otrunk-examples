                    </OTCompoundDoc>
                    <OTCompoundDoc id="fcebd776-7b9d-11de-9f61-b5789c46dafe" name="%fcebd776-7b9d-11de-9f61-b5789c46dafe" showEditBar="true">
                      <documentRefs>
                        <OTQuestion id="61d48387-7b9e-11de-9f61-b5789c46dafe" name="Success">
                          <prompt>
                            <OTCompoundDoc>
                              <bodyText>Were you able to rescue the shuttle?</bodyText>
                            </OTCompoundDoc>
                          </prompt>
                          <input>
                            <OTChoice name="Multiple choice" horizontal="true" alignContent="0">
                              <choices>
                                <OTCompoundDoc name="Yes">
                                  <bodyText>Yes</bodyText>
                                </OTCompoundDoc>
                                <OTCompoundDoc name="No">
                                  <bodyText>No</bodyText>
                                </OTCompoundDoc>
                              </choices>
                            </OTChoice>
                          </input>
                        </OTQuestion>
                        <OTQuestion id="d33fe7df-7bae-11de-96cd-839ce163f942" name="Strategy">
                          <prompt>
                            <OTCompoundDoc>
                              <bodyText>If you fire the Rocket twice in the same direction will the Rocket go</bodyText>
                            </OTCompoundDoc>
                          </prompt>
                          <input>
                            <OTChoice name="Multiple choice" horizontal="false" alignContent="0">
                              <choices>
                                <OTCompoundDoc name="the same speed as firing once">
