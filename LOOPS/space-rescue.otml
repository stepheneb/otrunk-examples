                                </OTCompoundDoc>
                                <OTCompoundDoc name="faster than firing once">
                                  <bodyText>faster than firing once</bodyText>
                                </OTCompoundDoc>
                                <OTCompoundDoc name="slower than firing once">
                                  <bodyText>slower than firing once</bodyText>
                                </OTCompoundDoc>
                              </choices>
                            </OTChoice>
                          </input>
                        </OTQuestion>
                        <OTQuestion id="b7fb8252-7baf-11de-96cd-839ce163f942" name="Question">
