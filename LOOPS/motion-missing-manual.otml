                        <OTQuestion id="881a8fa9-5e0a-11df-a1cd-d973855c6ef6" name="TryItOut1_1">
                          <prompt>
                            <OTCompoundDoc>
                              <bodyText>Do you think you can control the line on the graph by the way you move?</bodyText>
                            </OTCompoundDoc>
                          </prompt>
                          <input>
                            <OTChoice name="Multiple choice" horizontal="false" alignContent="0">
                              <choices>
                                <OTCompoundDoc id="c7f5c285-5e0a-11df-a1cd-d973855c6ef6" name="Yes">
                                  <bodyText>Yes</bodyText>
                                </OTCompoundDoc>
                                <OTCompoundDoc id="3e0bdbf2-5e0b-11df-a1cd-d973855c6ef6" name="No">
