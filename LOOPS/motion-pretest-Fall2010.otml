                                    <OTGraphSegmentCriterion property="ENDING_Y" operation="EQUAL_TO" expectedValue="2" tolerance="0.4" points="1" />
                                    <OTGraphSegmentCriterion property="SLOPE" operation="EQUAL_TO" expectedValue="0" tolerance="0.2" points="1" />
