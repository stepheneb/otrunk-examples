                                  </criteria>
                                </OTGraphSegment>
                                <OTGraphSegment>
                                  <criteria>
                                    <OTGraphSegmentCriterion property="BEGINNING_X" operation="EQUAL_TO" expectedValue="2" tolerance="1.25" points="0.5" />
                                    <OTGraphSegmentCriterion property="BEGINNING_Y" operation="EQUAL_TO" expectedValue="2" tolerance="0.4" points="0.5" />
                                    <OTGraphSegmentCriterion property="ENDING_X" operation="EQUAL_TO" expectedValue="5" tolerance="1.25" points="1" />
                                    <OTGraphSegmentCriterion property="SLOPE" operation="GREATER_THAN" expectedValue="0" tolerance="0" points="1" />
                                  </criteria>
                                </OTGraphSegment>
                                <OTGraphSegment>
                                  <criteria>
                                    <OTGraphSegmentCriterion property="BEGINNING_X" operation="EQUAL_TO" expectedValue="5" tolerance="1.25" points="1" />
                                    <OTGraphSegmentCriterion property="ENDING_X" operation="EQUAL_TO" expectedValue="7" tolerance="1.25" points="1" />
                                    <OTGraphSegmentCriterion property="SLOPE" operation="EQUAL_TO" expectedValue="0" tolerance="0.2" points="1" />
                                  </criteria>
                                </OTGraphSegment>
                                <OTGraphSegment>
                                  <criteria>
                                    <OTGraphSegmentCriterion property="BEGINNING_X" operation="EQUAL_TO" expectedValue="7" tolerance="1.25" points="1" />
                                    <OTGraphSegmentCriterion property="ENDING_Y" operation="EQUAL_TO" expectedValue="6" tolerance="0.4" points="1" />
                                    <OTGraphSegmentCriterion property="SLOPE" operation="GREATER_THAN" expectedValue="0" tolerance="0" points="1" />
                                  </criteria>
                                </OTGraphSegment>
                                <OTGraphSegment>
                                  <criteria>
                                    <OTGraphSegmentCriterion property="BEGINNING_Y" operation="EQUAL_TO" expectedValue="6" tolerance="0.4" points="1" />
                                    <OTGraphSegmentCriterion property="DELTA_X" operation="EQUAL_TO" expectedValue="3" tolerance="1.25" points="1" />
                                    <OTGraphSegmentCriterion property="SLOPE" operation="EQUAL_TO" expectedValue="0" tolerance="0.2" points="1" />
                                  </criteria>
                                </OTGraphSegment>
                                <OTGraphSegment>
                                  <criteria>
