                              <br />
                              <br />
                            </div>
                          </div>
                        </div>
                      </bodyText>
                      <documentRefs>
                        <OTQuestion id="f95c4a75-52d9-11df-a5a4-e351bf59433a" name="FootballMC">
                          <prompt>
                            <OTCompoundDoc>
                              <bodyText>The red team makes a pass from their own 20 yard line to the blue team's 40 yard line.  How long was the pass?</bodyText>
                            </OTCompoundDoc>
                          </prompt>
                          <input>
                            <OTChoice name="Multiple choice" horizontal="false" alignContent="1" autoLetter="false">
                              <choices>
                                <OTCompoundDoc id="dfb15355-52db-11df-a5a4-e351bf59433a" name="20 yards">
                                  <bodyText>20 yards</bodyText>
                                </OTCompoundDoc>
                                <OTCompoundDoc id="63a09263-52da-11df-a5a4-e351bf59433a" name="30 yards">
                                  <bodyText>30 yards</bodyText>
                                </OTCompoundDoc>
                                <OTCompoundDoc id="76b01c58-52dc-11df-8ac5-99db81b82b35" name="40 yards">
                                  <bodyText>40 yards</bodyText>
                                </OTCompoundDoc>
                                <OTCompoundDoc id="76b01c59-52dc-11df-8ac5-99db81b82b35" name="50 yards">
                                  <bodyText>50 yards</bodyText>
                                </OTCompoundDoc>
                                <OTCompoundDoc id="5f424151-52da-11df-a5a4-e351bf59433a" name="20 yards">
                                  <bodyText>60 yards</bodyText>
                                </OTCompoundDoc>
                              </choices>
                            </OTChoice>
                          </input>
                          <correctAnswer>
                            <OTObjectSet>
                              <objects>
                                <OTCompoundDoc id="73818367-52da-11df-a5a4-e351bf59433a" name="50 yards">
                                  <bodyText>40 yards</bodyText>
                                </OTCompoundDoc>
                              </objects>
                            </OTObjectSet>
                          </correctAnswer>
                        </OTQuestion>
