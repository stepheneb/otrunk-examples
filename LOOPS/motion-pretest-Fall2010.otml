                                    <OTGraphSegmentCriterion property="BEGINNING_Y" operation="EQUAL_TO" expectedValue="2" tolerance="0.4" points="0.5" />
                                    <OTGraphSegmentCriterion property="ENDING_X" operation="EQUAL_TO" expectedValue="2" tolerance="1.25" points="0.5" />
                                    <OTGraphSegmentCriterion property="ENDING_Y" operation="EQUAL_TO" expectedValue="2" tolerance="0.4" points="0.5" />
