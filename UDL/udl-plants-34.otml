                            <OTUDLQuestion id="e138c422-2361-11dd-a8ce-53b5f17aa15a" name="Question" showConvertButton="true">
                              <prompt>
                                <OTCompoundDoc>
                                  <bodyText>a) Rebecca knows that plants need light to grow. Why do plants need light to grow?</bodyText>
                                </OTCompoundDoc>
                              </prompt>
                              <input>
                                <OTText text="" />
                              </input>
                              <activityReferences>
                                <OTReference annotation="ACTIVITY_REFERENCE">
                                  <referencedObject>
                                    <object refid="${section_7}" />
                                  </referencedObject>
                                </OTReference>
                                <OTReference annotation="ACTIVITY_REFERENCE">
                                  <referencedObject>
                                    <object refid="${section_4}" />
                                  </referencedObject>
                                </OTReference>
                                <OTReference annotation="ACTIVITY_REFERENCE">
                                  <referencedObject>
                                    <object refid="${section_5}" />
                                  </referencedObject>
                                </OTReference>
                              </activityReferences>
                              <correctAnswer>
                                <OTCompoundDoc>
                                  <bodyText>Light energy that hits the leaves is needed for photosynthesis. This process produces the chemical energy plants use to grow.</bodyText>
                                </OTCompoundDoc>
                              </correctAnswer>
                            </OTUDLQuestion>
