                        <object refid="${stems-roots-leaves-page-4}" />
                        <OTUDLCompoundDoc local_id="stems-page-3">
                          <bodyText>
                            <div class="buffer">
                              <div class="border">
                                <div class="body">
                                  <div class="subtitle">Explore</div>
                                  Make observation about your plant by answering the following 
          questions.
                                  <br />
                                  <br />
                                  <br />
                                  <object refid="860b0c0c-e48b-11dc-a385-b75cbeebcd94" />
                                  <br />
                                  <br />
                                  <br />
                                  <object refid="927e9ab9-e48b-11dc-a385-b75cbeebcd94" />
                                  <br />
                                  <br />
                                  <br />
                                  <object refid="9b1a6e76-e48b-11dc-a385-b75cbeebcd94" />
                                  <br />
                                  <br />
                                  <br />
                                  <object refid="ab6e6cab-e48b-11dc-a385-b75cbeebcd94" />
                                  <br />
                                  <br />
                                </div>
                              </div>
                            </div>
                          </bodyText>
                          <documentRefs>
                            <OTUDLQuestion id="860b0c0c-e48b-11dc-a385-b75cbeebcd94" name="Question" showConvertButton="true">
                              <prompt>
                                <OTUDLCompoundDoc>
                                  <bodyText>1. How many stems does your plant have?</bodyText>
                                </OTUDLCompoundDoc>
                              </prompt>
                              <input>
                                <OTText />
                              </input>
                            </OTUDLQuestion>
                            <OTUDLQuestion id="927e9ab9-e48b-11dc-a385-b75cbeebcd94" name="Question" showConvertButton="true">
                              <prompt>
                                <OTUDLCompoundDoc>
                                  <bodyText>2. How many branches does your plant have?</bodyText>
                                </OTUDLCompoundDoc>
                              </prompt>
