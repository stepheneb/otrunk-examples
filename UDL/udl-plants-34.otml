                            <OTUDLQuestion id="eb41ce46-6dfb-11dd-afb3-212dca4fd45c" name="Question" showConvertButton="true">
                              <prompt>
                                <OTCompoundDoc>
                                  <bodyText>Describe the character of the stem: stiff, weak, rough, smooth, etc.</bodyText>
                                </OTCompoundDoc>
