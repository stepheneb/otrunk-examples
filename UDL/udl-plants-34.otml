                                      <prompt>
                                        <OTCompoundDoc>
                                          <bodyText>Why does a plant need leaves?</bodyText>
                                        </OTCompoundDoc>
                                      </prompt>
                                      <input>
                                        <OTText />
                                      </input>
                                      <correctAnswer>
                                        <OTCompoundDoc>
                                          <bodyText>The leaves collect energy from sunlight. They make sugar through photosynthesis.</bodyText>
                                        </OTCompoundDoc>
                                      </correctAnswer>
                                    </OTUDLQuestion>
                                  </documentRefs>
                                  <bodyText>
                                    <object refid="d508c559-67bb-11dd-9a15-9f42c4cb155d" />
                                    <br />
                                  </bodyText>
                                </OTUDLCompoundDoc>
                              </cards>
