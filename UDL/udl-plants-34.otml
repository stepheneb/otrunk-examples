                            <OTUDLQuestion id="b735c94b-67c0-11dd-9a15-9f42c4cb155d" name="Question" showConvertButton="true">
                              <prompt>
                                <OTCompoundDoc>
                                  <bodyText>How is a cactus different from a regular leafy plant, so that it can survive in a desert?</bodyText>
                                </OTCompoundDoc>
                              </prompt>
                              <input>
                                <OTText />
                              </input>
