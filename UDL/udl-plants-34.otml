                        <OTUDLCompoundDoc local_id="stems-page-5" showDefinitions="true">
                          <bodyText>
                            <div class="buffer">
                              <div class="border">
                                <div class="body">
                                  <table width="100%">
                                    <tr>
                                      <td>
                                        <div class="subtitle" />
                                      </td>
                                      <td align="right">
                                        <div class="page-number">8</div>
                                      </td>
                                    </tr>
                                  </table>
                                  <object refid="ce7bf75a-e48b-11dc-a385-b75cbeebcd94" />
                                  <br />
                                  <br />
                                  <object refid="148f9571-8f2c-11dd-ae21-af44661cfdeb" />
                                  <br />
