                            <OTUDLQuestion id="f27ba108-6dfb-11dd-afb3-212dca4fd45c" name="Question" showConvertButton="true">
                              <prompt>
                                <OTCompoundDoc>
                                  <bodyText>Are there any buds found on the stem? If so, where are they?</bodyText>
