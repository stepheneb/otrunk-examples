                      <header>
                        <OTUDLCompoundDoc id="b91fca9c-7121-11dc-b75e-4b06cf421f29" showEditBar="false" name="b91fca9c-7121-11dc-b75e-4b06cf421f29">
                          <bodyText>
                            <div class="title">Plant story</div>
                          </bodyText>
                        </OTUDLCompoundDoc>
                      </header>
                      <footer>
                        <object refid="${nav-bar}" />
                      </footer>
                      <north>
                        <object refid="b91fca9c-7121-11dc-b75e-4b06cf421f29" />
                      </north>
                      <center>
                        <object refid="b91fca9b-7121-11dc-b75e-4b06cf421f29" />
