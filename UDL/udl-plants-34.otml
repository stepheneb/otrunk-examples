                          <documentRefs>
                            <OTUDLQuestion id="85572f64-e48c-11dc-9e1f-cd31bf397141" name="Question" showConvertButton="true">
                              <prompt>
                                <OTUDLCompoundDoc>
                                  <bodyText>Use your hand lens to take a closer look at the roots. Draw what you see. Label any interesting features. Take a snapshot of your drawing.</bodyText>
                                </OTUDLCompoundDoc>
                              </prompt>
                              <input>
                                <OTDrawingTool local_id="question_drawing_a" isVectorStyle="false" name="Drawing tool" />
                              </input>
