                              </coach>
                              <statement>
                                <OTUDLCompoundDoc>
                                  <bodyText>
                                    <div class="buffer">
                                      <div class="border">
                                        <div class="body">You can do this, too.  Try rubbing your hands together.</div>
                                      </div>
