                                <OTUDLQuestion name="L5" showConvertButton="false" showScaffolding="true">
                                  <prompt>
                                    <OTUDLCompoundDoc>
                                      <bodyText>Why do you think the friction force increased when the weight increased?</bodyText>
                                    </OTUDLCompoundDoc>
                                  </prompt>
                                  <input>
                                    <OTText rows="1" />
                                  </input>
                                  <correctAnswer>
                                    <OTCompoundDoc>
                                      <bodyText>Friction is proportional to the force between the surfaces. Doubling the weight also doubled the force.</bodyText>
                                    </OTCompoundDoc>
                                  </correctAnswer>
                                </OTUDLQuestion>
                                <OTUDLQuestion name="L4" showConvertButton="false" showScaffolding="true">
                                  <prompt>
                                    <OTUDLCompoundDoc>
                                      <bodyText>Why do you think the friction force increased when the weight increased?</bodyText>
