                              </prompt>
                              <input>
                                <OTChoice name="Multiple choice" horizontal="false" alignContent="0">
                                  <choices>
                                    <OTCompoundDoc name="The energy is stored in the bike.">
                                      <bodyText>The energy is stored in the bike.</bodyText>
                                    </OTCompoundDoc>
                                    <OTCompoundDoc id="8dacf4e1-812c-11de-99e5-97a703c05153" name="The brakes get hot.">
                                      <bodyText>The brakes get hot.</bodyText>
                                    </OTCompoundDoc>
                                    <OTCompoundDoc name="The energy just disappears.">
                                      <bodyText>The energy just disappears.</bodyText>
                                    </OTCompoundDoc>
                                    <OTCompoundDoc name="The molecules in the brakes slow down.">
                                      <bodyText>The molecules in the brakes slow down.</bodyText>
                                    </OTCompoundDoc>
                                  </choices>
                                </OTChoice>
                              </input>
                              <correctAnswer>
                                <OTObjectSet>
                                  <objects>
                                    <object refid="8dacf4e1-812c-11de-99e5-97a703c05153" />
                                  </objects>
