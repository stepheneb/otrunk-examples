                              </coach>
                              <statement>
                                <OTUDLCompoundDoc>
                                  <bodyText>
                                    <div class="buffer">
                                      <div class="border">
                                        <div class="body">Wow, the kids can all move like Spiderman!</div>
                                      </div>
                                    </div>
                                    <img src="http://udl.concord.org/share/shared-images/blah-anim.gif" height="150" width="200" />
                                  </bodyText>
                                </OTUDLCompoundDoc>
                              </statement>
