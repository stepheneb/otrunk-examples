                          <documentRefs>
                            <OTUDLQuestion id="5c0b4ff8-e3d0-11dc-bd8e-e5ab906ad868" name="Question" showConvertButton="true">
                              <prompt>
                                <OTUDLCompoundDoc>
                                  <bodyText>3. Heat is a form of energy, so it takes energy to heat up a penny. Where did the heat come from?</bodyText>
                                </OTUDLCompoundDoc>
                              </prompt>
                              <input>
                                <OTText />
                              </input>
                              <correctAnswer>
                                <OTCompoundDoc>
                                  <bodyText>It came from you pushing the penny.</bodyText>
                                </OTCompoundDoc>
                              </correctAnswer>
                            </OTUDLQuestion>
                            <OTUDLQuestion id="a82a4fef-e3d0-11dc-bd8e-e5ab906ad868" name="Question" showConvertButton="true">
                              <prompt>
                                <OTUDLCompoundDoc>
                                  <bodyText>4. Explain why the heating effect of rubbing fast would be different from the heating effect of rubbing slowly.</bodyText>
                                </OTUDLCompoundDoc>
                              </prompt>
                              <input>
                                <OTText />
                              </input>
                              <correctAnswer>
                                <OTCompoundDoc>
                                  <bodyText>Faster motion has more energy that can be converted into heat.</bodyText>
                                </OTCompoundDoc>
                              </correctAnswer>
                            </OTUDLQuestion>
                            <OTUDLQuestion id="b749c5bc-e3d0-11dc-bd8e-e5ab906ad868" name="Question" showConvertButton="true">
                              <prompt>
                                <OTUDLCompoundDoc>
                                  <bodyText>5. Explain why the heating effect of pushing hard would be different from the heating effect of pushing lightly.</bodyText>
                                </OTUDLCompoundDoc>
                              </prompt>
                              <input>
                                <OTText />
                              </input>
                              <correctAnswer>
                                <OTCompoundDoc>
                                  <bodyText>Pushing down hard creates more friction, hence more heating.</bodyText>
                                </OTCompoundDoc>
                              </correctAnswer>
                            </OTUDLQuestion>
                          </documentRefs>
                        </OTUDLCompoundDoc>
