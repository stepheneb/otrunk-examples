                            <OTUDLQuestion id="4dfdb4e7-812c-11de-99e5-97a703c05153" name="Question" showConvertButton="true">
                              <prompt>
                                <OTCompoundDoc>
                                  <bodyText>1. You see a hockey puck sliding on ice. What makes it slow down?</bodyText>
                                </OTCompoundDoc>
                              </prompt>
                              <input>
                                <OTChoice name="Multiple choice" horizontal="false" alignContent="0">
                                  <choices>
                                    <OTCompoundDoc name="It runs out of push.">
