                              <prompt>
                                <OTCompoundDoc>
                                  <bodyText>1. You see a hockey puck sliding on ice. What makes it slow down?</bodyText>
                                </OTCompoundDoc>
                              </prompt>
                              <input>
                                <OTChoice name="Multiple choice" horizontal="false" alignContent="0">
                                  <choices>
