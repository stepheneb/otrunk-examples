                              <prompt>
                                <OTUDLCompoundDoc>
                                  <bodyText>Which would you rather visit, Slippery World, or Sticky World? Why?</bodyText>
                                </OTUDLCompoundDoc>
                              </prompt>
                              <input>
                                <OTText />
                              </input>
                            </OTUDLQuestion>
                          </documentRefs>
