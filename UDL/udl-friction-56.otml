                            <OTUDLCoachStatement>
                              <coach>
                                <object refid="${character_affective}" />
                              </coach>
                              <statement>
                                <OTUDLCompoundDoc>
                                  <bodyText>
                                    <div class="buffer">
                                      <div class="border">
                                        <div class="body">Wow, Maria and Eduardo went sliding a long distance!</div>
                                      </div>
