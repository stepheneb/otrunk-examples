                                <OTChoice name="Multiple choice" alignContent="0" horizontal="false">
                                  <choices>
                                    <OTCompoundDoc name="Friction can only slow things down.">
                                      <bodyText>Friction can only slow things down.</bodyText>
                                    </OTCompoundDoc>
                                    <OTCompoundDoc id="18d1900e-fc12-11dc-afb1-bfd5bcc79158/input/choices[1]" name="Friction is never useful.">
                                      <bodyText>Friction is never useful.</bodyText>
                                    </OTCompoundDoc>
