                            <OTUDLCoachStatement>
                              <coach>
                                <object refid="${character_strategic}" />
                              </coach>
                              <statement>
                                <OTUDLCompoundDoc>
                                  <bodyText>
                                    <div class="buffer">
                                      <div class="border">
                                        <div class="body">We need to find the force required to push the box with one water bottle.</div>
                                      </div>
                                    </div>
                                  </bodyText>
                                </OTUDLCompoundDoc>
