                            <OTUDLCoachStatement>
                              <coach>
                                <object refid="${character_affective}" />
                              </coach>
                              <statement>
                                <OTUDLCompoundDoc>
                                  <bodyText>
                                    <div class="buffer">
                                      <div class="border">
                                        <div class="body">Have you ever taken a temperature at home? What did you use to take the temperature?</div>
                                      </div>
                                    </div>
                                    <img src="jres:///org/concord/otrunk/udl/document/blah-anim.gif" height="150" width="200" />
                                  </bodyText>
                                </OTUDLCompoundDoc>
                              </statement>
                            </OTUDLCoachStatement>
                            <OTUDLCoachStatement>
                              <coach>
                                <object refid="${character_strategic}" />
                              </coach>
                              <statement>
                                <OTUDLCompoundDoc>
                                  <bodyText>
                                    <div class="buffer">
                                      <div class="border">
                                        <div class="body">You need to find the temperature of the penny as you rub it on a block of wood.  The temperature sensor allows you to do this.</div>
                                      </div>
                                    </div>
