                                <OTUDLCompoundDoc>
                                  <bodyText>
                                    Write a summary of Chapter 8.
                                    <br />
                                    <br />
                                  </bodyText>
                                </OTUDLCompoundDoc>
                              </prompt>
                              <input>
                                <OTText />
