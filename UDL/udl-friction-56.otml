                                <OTQuestion name="L4">
                                  <prompt>
                                    <OTUDLCompoundDoc>
                                      <bodyText>Why do you think this was true?</bodyText>
                                    </OTUDLCompoundDoc>
                                  </prompt>
                                  <input>
                                    <OTUDLCompoundDoc>
                                      <bodyText>
                                        <object refid="2953e3c4-78f0-11dc-8275-3da1b6a57fd9" />
                                        <p />
                                        Think which surface is more slippery.
                                      </bodyText>
                                      <documentRefs>
