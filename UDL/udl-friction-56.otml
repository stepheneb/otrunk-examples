                                  <bodyText>
                                    Suppose there were
                                    <b>no</b>
                                    friction between the two surfaces in this model. Choose all of the statements that would be true.
                                  </bodyText>
                                </OTUDLCompoundDoc>
                              </prompt>
                              <input>
                                <OTChoiceMultiple name="Multiple choice (check boxes)" alignContent="0" horizontal="false">
                                  <choices>
                                    <OTCompoundDoc name="The energy would start as kinetic energy and gradually be changed to heat energy.">
                                      <bodyText>The energy would start as kinetic energy and gradually be changed to heat energy.</bodyText>
                                    </OTCompoundDoc>
                                    <OTCompoundDoc id="cab6f75c-e3e0-11dc-beaa-cdc9f3ab4400/input/choices[1]" name="The object would never slow down.">
                                      <bodyText>The object would never slow down.</bodyText>
                                    </OTCompoundDoc>
                                    <OTCompoundDoc name="The object would still slow down eventually.">
                                      <bodyText>The object would still slow down eventually.</bodyText>
                                    </OTCompoundDoc>
                                    <OTCompoundDoc id="cab6f75c-e3e0-11dc-beaa-cdc9f3ab4400/input/choices[3]" name="The box surface would not heat up at all.">
                                      <bodyText>The box surface would not heat up at all.</bodyText>
                                    </OTCompoundDoc>
                                  </choices>
                                </OTChoiceMultiple>
                              </input>
                              <correctAnswer>
                                <OTObjectSet>
                                  <objects>
                                    <object refid="cab6f75c-e3e0-11dc-beaa-cdc9f3ab4400/input/choices[1]" />
