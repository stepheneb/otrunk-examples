                        <OTUDLCompoundDoc name="%60535c18-21f6-11dd-883b-015dfae2d4ec" showDefinitions="true">
                          <documentRefs>
                            <OTCardContainer id="385228b3-625a-11dd-818b-499ca14d14a5">
                              <cards>
                                <OTUDLCompoundDoc id="385228b5-625a-11dd-818b-499ca14d14a5">
                                  <bodyText>
                                    Press the autoscale button
                                    <img src="http://udl.concord.org/artwork/technical_hints/autoscalelabel-icon.jpg" />
                                    to change the temperature scale so that you can 
    easily see the change in temperature of the penny.
                                  </bodyText>
                                </OTUDLCompoundDoc>
                                <OTUDLCompoundDoc id="41c28a76-625a-11dd-818b-499ca14d14a5" name="%41c28a76-625a-11dd-818b-499ca14d14a5" showEditBar="true">
                                  <bodyText>
                                    Using the label tool
                                    <img src="http://udl.concord.org/artwork/technical_hints/datapointlabel-icon.jpg" />
                                    , label the part where you were rubbing slowly. Label 
    the part where you were rubbing fast. Then take a snapshot of your graph.
                                  </bodyText>
                                </OTUDLCompoundDoc>
                                <OTUDLCompoundDoc id="63becc20-625a-11dd-818b-499ca14d14a5" name="%63becc20-625a-11dd-818b-499ca14d14a5" showEditBar="true">
                                  <bodyText>
                                    <object refid="a69348cf-21f4-11dd-883b-015dfae2d4ec" />
                                  </bodyText>
                                  <documentRefs>
                                    <OTUDLQuestion id="a69348cf-21f4-11dd-883b-015dfae2d4ec" name="Question" showConvertButton="true">
                                      <prompt>
                                        <OTCompoundDoc>
                                          <bodyText>1. Describe the graph. Which had more heating effect, rubbing slowly or rubbing fast?</bodyText>
                                        </OTCompoundDoc>
                                      </prompt>
                                      <input>
                                        <OTText />
                                      </input>
                                      <correctAnswer>
                                        <OTCompoundDoc>
                                          <bodyText>Rubbing fast should show more heating effect.</bodyText>
                                        </OTCompoundDoc>
                                      </correctAnswer>
                                    </OTUDLQuestion>
                                  </documentRefs>
                                </OTUDLCompoundDoc>
                                <OTUDLCompoundDoc id="6c1b73f6-625a-11dd-818b-499ca14d14a5" name="%6c1b73f6-625a-11dd-818b-499ca14d14a5" showEditBar="true">
                                  <bodyText>
                                    <object refid="461b52d0-21f6-11dd-883b-015dfae2d4ec" />
                                  </bodyText>
                                  <documentRefs>
                                    <OTUDLQuestion id="461b52d0-21f6-11dd-883b-015dfae2d4ec" name="Question" showConvertButton="true">
                                      <prompt>
                                        <OTCompoundDoc>
                                          <bodyText>2. How can you tell from the graph which had more heating effect?</bodyText>
                                        </OTCompoundDoc>
                                      </prompt>
                                      <input>
                                        <OTText />
                                      </input>
                                      <correctAnswer>
                                        <OTCompoundDoc>
                                          <bodyText>The line will be steeper (faster temperature rise).</bodyText>
                                        </OTCompoundDoc>
                                      </correctAnswer>
                                    </OTUDLQuestion>
                                  </documentRefs>
                                </OTUDLCompoundDoc>
                              </cards>
