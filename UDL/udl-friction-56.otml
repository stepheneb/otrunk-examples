                                          <choices>
                                            <OTCompoundDoc name="Keeping the outlier">
                                              <bodyText>Keeping the outlier</bodyText>
                                            </OTCompoundDoc>
                                            <OTCompoundDoc id="81e43f33-e3e4-11dc-beaa-cdc9f3ab4400/input/choices[1]" name="Discarding the outlier">
                                              <bodyText>Discarding the outlier in this case, because it's so far from the others.</bodyText>
                                            </OTCompoundDoc>
                                          </choices>
                                        </OTChoice>
                                      </input>
                                      <correctAnswer>
