                            <OTUDLQuestion id="36cc13ae-fba2-11dc-8b26-87d692f0bb5d" name="Question" showConvertButton="true">
                              <prompt>
                                <OTUDLCompoundDoc>
                                  <bodyText>2. A person on a bicycle uses brakes to slow down. Where does the bicycle's energy of motion go when it slows down?</bodyText>
                                </OTUDLCompoundDoc>
                              </prompt>
                              <input>
                                <OTChoice name="Multiple choice" alignContent="0" horizontal="false">
                                  <choices>
                                    <OTCompoundDoc name="The energy is stored in the bike.">
                                      <bodyText>The energy is stored in the bike.</bodyText>
                                    </OTCompoundDoc>
                                    <OTCompoundDoc id="36cc13ae-fba2-11dc-8b26-87d692f0bb5d/input/choices[1]" name="The brakes get hot.">
                                      <bodyText>The brakes get hot.</bodyText>
                                    </OTCompoundDoc>
                                    <OTCompoundDoc name="The energy just disappears. ">
                                      <bodyText>The energy just disappears.</bodyText>
                                    </OTCompoundDoc>
                                    <OTCompoundDoc name="The molecules in the brakes slow down.">
