                <OTUDLSection local_id="section_8" name="Measuring challenge" description="Analyze data from friction experiments." isComputerBased="false" isIntroduction="false" isMath="true" isPosttest="false" isPretest="false" isReading="false" isWrapup="false">
                  <content>
                    <OTCardContainer id="e5837c4b-7e82-11dc-a02a-2fa3fd422b5e" contentsMayScroll="true" loadViewOnlyOnDemand="true" closeViewOnExit="false">
                      <cards>
                        <OTUDLCompoundDoc id="e5837c4a-7e82-11dc-a02a-2fa3fd422b5e" showDefinitions="true">
                          <bodyText>
                            <div class="buffer">
