                          <coachStatements>
                            <OTUDLCoachStatement>
                              <coach>
                                <object refid="${character_affective}" />
                              </coach>
                              <statement>
                                <OTUDLCompoundDoc>
                                  <bodyText>
                                    <div class="buffer">
                                      <div class="border">
                                        <div class="body">I've weighed myself at home.  Sometimes I weigh produce at the supermarket.</div>
                                      </div>
                                    </div>
                                    <img src="http://udl.concord.org/share/shared-images/blah-anim.gif" height="150" width="200" />
                                  </bodyText>
                                </OTUDLCompoundDoc>
                              </statement>
                            </OTUDLCoachStatement>
                            <OTUDLCoachStatement>
                              <coach>
                                <object refid="${character_strategic}" />
                              </coach>
                              <statement>
                                <OTUDLCompoundDoc>
                                  <bodyText>
                                    <div class="buffer">
