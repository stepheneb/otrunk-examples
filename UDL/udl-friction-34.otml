                                        </tr>
                                      </table>
                                      <p />
                                    </li>
                                  </ol>
                                </div>
                              </div>
                            </div>
                          </bodyText>
                          <documentRefs>
                            <object refid="${act1-elab-text1}" />
                            <object refid="${act1-elab-text2}" />
                            <object refid="${act1-elab-text3}" />
                            <object refid="${act1-elab-text4}" />
                          </documentRefs>
                          <coachStatements>
                            <OTUDLCoachStatement>
