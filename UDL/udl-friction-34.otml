                          <documentRefs>
                            <OTUDLQuestions local_id="act2-explain-ques1">
                              <questions>
                                <OTQuestion local_id="act2-explain-ques1-5" name="L5">
                                  <prompt>
                                    <OTUDLCompoundDoc>
                                      <bodyText>
                                        <div class="body-no-border">Did the child slide on all of the surface materials?</div>
                                      </bodyText>
                                    </OTUDLCompoundDoc>
                                  </prompt>
                                  <input>
                                    <OTUDLCompoundDoc>
                                      <documentRefs>
                                        <OTText local_id="act2-ext-ques4-5-1" />
                                      </documentRefs>
