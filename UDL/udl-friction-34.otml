                            </OTUDLCompoundDoc>
                            <OTUDLCompoundDoc name="Exploring the question 2" showDefinitions="true">
                              <bodyText>
                                <div class="buffer">
                                  <div class="border">
                                    <div class="body">
                                      <table width="100%">
                                        <tr>
                                          <td>
                                            <div class="subtitle" />
                                          </td>
                                          <td align="right">
                                            <div class="page-number">3</div>
                                          </td>
                                        </tr>
                                      </table>
                                      <table>
                                        <tr>
                                          <td>
                                            Describe what it would feel like to walk around if the bottoms of your shoes were sticky like gum. Use pictures or words.
                                            <br />
                                            <br />
                                          </td>
                                          <td width="300">
                                            <object refid="${introtext3}" />
                                          </td>
                                        </tr>
                                      </table>
                                      <object refid="${introdrawing2}" />
                                      <p />
                                      <table>
                                        <tr>
                                          <td>Name another surface that is very, very sticky!</td>
                                          <td width="300">
                                            <object refid="${introtext4}" />
                                          </td>
                                        </tr>
                                      </table>
                                    </div>
                                  </div>
                                </div>
                              </bodyText>
                              <documentRefs>
                                <OTText local_id="introtext3" rows="2" />
                                <OTDrawingTool2 local_id="introdrawing2">
                                  <graphables>
                                    <OTDrawingShape type="" fillType="clear" x="0.0" y="0.0" width="0.0" height="0.0" color="0x000000" />
                                    <OTDrawingShape type="" fillType="clear" x="0.0" y="0.0" width="0.0" height="0.0" color="0x000000" />
                                    <OTDrawingShape type="" fillType="clear" x="0.0" y="0.0" width="0.0" height="0.0" color="0x000000" />
                                    <OTDrawingShape type="" fillType="clear" x="0.0" y="0.0" width="0.0" height="0.0" color="0x000000" />
                                    <OTDrawingShape type="" fillType="clear" x="0.0" y="0.0" width="0.0" height="0.0" color="0x000000" />
                                  </graphables>
                                </OTDrawingTool2>
                                <OTText local_id="introtext4" />
                              </documentRefs>
                            </OTUDLCompoundDoc>
                          </cards>
