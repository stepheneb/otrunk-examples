                            <OTUDLQuestion id="0bb21154-e483-11dc-b779-671ad29357cb" name="Question" showConvertButton="true">
                              <prompt>
                                <OTUDLCompoundDoc>
                                  <bodyText>1) How can you measure the amount of friction between two surfaces?</bodyText>
                                </OTUDLCompoundDoc>
                              </prompt>
                              <input>
                                <OTText name="Open response"/>
                              </input>
                              <activityReferences>
