                                        <OTUDLQuestion id="cfefc5c7-67e2-11dd-8f1b-0725e6f8772c" name="Question" showConvertButton="true">
                                          <prompt>
                                            <OTCompoundDoc>
                                              <bodyText>How long do you think the plant will stay alive?</bodyText>
                                            </OTCompoundDoc>
                                          </prompt>
                                          <input>
                                            <OTChoice name="Multiple choice" alignContent="0">
                                              <choices>
                                                <OTCompoundDoc name="It will die quickly.">
                                                  <bodyText>It will die quickly.</bodyText>
                                                </OTCompoundDoc>
                                                <OTCompoundDoc name="A few days.">
                                                  <bodyText>A few days.</bodyText>
                                                </OTCompoundDoc>
                                                <OTCompoundDoc id="cfefc5c7-67e2-11dd-8f1b-0725e6f8772c/input/choices[2]" name="A few weeks.">
                                                  <bodyText>A few weeks.</bodyText>
                                                </OTCompoundDoc>
                                                <OTCompoundDoc name="A month or more.">
                                                  <bodyText>A month or more.</bodyText>
                                                </OTCompoundDoc>
                                              </choices>
                                            </OTChoice>
                                          </input>
                                          <correctAnswer>
                                            <object refid="cfefc5c7-67e2-11dd-8f1b-0725e6f8772c/input/choices[2]"/>
                                          </correctAnswer>
                                        </OTUDLQuestion>
                                      </documentRefs>
                                      <bodyText>
