                            <OTSoundPlayer id="ac4202c8-f135-11dc-b7d2-cd2b75b4ce4b" name="Sound player" src="http://udl.concord.org/sound/friction_56/fr_56_ch6_p12.mp3" />
                            <OTUDLQuestion id="4bd8dd4d-e3c8-11dc-9fd4-5fee575808b0" name="Question" showConvertButton="true">
                              <prompt>
                                <OTUDLCompoundDoc>
                                  <bodyText>
                                    Write a summary of Chapter 6.
                                    <br />
                                    <br />
                                  </bodyText>
                                </OTUDLCompoundDoc>
                              </prompt>
                              <input>
                                <OTText />
                              </input>
