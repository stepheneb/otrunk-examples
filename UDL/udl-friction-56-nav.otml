                  <content>
                    <OTCardContainer id="0be2278c-78d5-11dc-82f7-4bde662316f0" contentsMayScroll="true" loadViewOnlyOnDemand="true" closeViewOnExit="false">
                      <cards>
                        <OTUDLCompoundDoc id="0be2278b-78d5-11dc-82f7-4bde662316f0" name="%0be2278b-78d5-11dc-82f7-4bde662316f0" showDefinitions="true">
                          <bodyText>
                            <div class="buffer">
                              <div class="border">
                                <div class="body">
                                  <table width="100%">
                                    <tr>
                                      <td>
                                        <div class="subtitle" />
