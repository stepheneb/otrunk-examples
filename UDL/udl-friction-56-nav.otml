                              <statement>
                                <OTUDLCompoundDoc>
                                  <bodyText>
                                    <div class="buffer">
                                      <div class="border">
                                        <div class="body">What would a world without friction be like?</div>
                                      </div>
                                    </div>
                                  </bodyText>
