                            <object refid="2b9ab66c-775f-11dc-88e0-f91e4efaa30a" />
                          </currentCard>
                        </OTCardContainer>
                      </content>
                      <header>
                        <OTUDLCompoundDoc id="2b9ab66e-775f-11dc-88e0-f91e4efaa30a" showEditBar="false" name="%2b9ab66e-775f-11dc-88e0-f91e4efaa30a">
                          <bodyText>
                            <div class="title">Electrical birthday party</div>
                          </bodyText>
                        </OTUDLCompoundDoc>
                      </header>
                      <footer>
                        <object refid="${nav-bar}" />
                      </footer>
                      <north>
                        <object refid="2b9ab66e-775f-11dc-88e0-f91e4efaa30a" />
                      </north>
                      <center>
                        <object refid="2b9ab66d-775f-11dc-88e0-f91e4efaa30a" />
                      </center>
                      <south>
                        <object refid="${nav-bar}" />
                      </south>
                    </OTUDLSection>
                    <OTUDLSection local_id="section_4" name="Charged balloons" isComputerBased="true" isIntroduction="false" isMath="false" isPosttest="false" isPretest="false" isReading="false" isWrapup="false" description="How do electric charges behave?" goals="">
                      <content>
                        <OTCardContainer id="1b062cc5-775f-11dc-88e0-f91e4efaa30a" contentsMayScroll="true" loadViewOnlyOnDemand="true" closeViewOnExit="true">
                          <cards>
                            <OTUDLCompoundDoc id="1b062cc4-775f-11dc-88e0-f91e4efaa30a" name="%1b062cc4-775f-11dc-88e0-f91e4efaa30a" showDefinitions="true">
                              <bodyText>
                                <div class="buffer">
                                  <div class="border">
                                    <div class="body">
                                      <table width="100%">
                                        <tr>
                                          <td>
                                            <div class="subtitle" />
                                          </td>
                                          <td align="right">
                                            <div class="page-number">1</div>
                                          </td>
                                        </tr>
                                      </table>
                                      <table width="100%">
                                        <tr>
                                          <td style="padding:40px">
                                            <img src="http://udl.concord.org/artwork/elect_34/charged_balloons/el_34_charged_balloons.png" border="0" />
                                          </td>
                                          <td>
                                            In this activity, you will place electric charges on balloons and 
          see what they do.
                                            <br />
                                            Then you will compare this to a
                                            <b>model</b>
                                            of 
          charged balloons.
                                          </td>
                                        </tr>
                                      </table>
                                      <blockquote />
                                      <br />
                                    </div>
                                  </div>
                                </div>
                              </bodyText>
                              <coachStatements>
                                <OTUDLCoachStatement>
                                  <coach>
                                    <object refid="${character_affective}" />
                                  </coach>
                                  <statement>
                                    <OTUDLCompoundDoc>
                                      <bodyText>
                                        <div class="buffer">
                                          <div class="border">
                                            <div class="body">I've played with balloons at parties.  I've rubbed them on my hair and clothes.</div>
                                          </div>
                                        </div>
                                        <img src="http://udl.concord.org/share/shared-images/blah-anim.gif" height="150" width="200" />
                                      </bodyText>
                                    </OTUDLCompoundDoc>
                                  </statement>
                                </OTUDLCoachStatement>
                                <OTUDLCoachStatement>
                                  <coach>
                                    <object refid="${character_recognition}" />
                                  </coach>
                                  <statement>
                                    <OTUDLCompoundDoc>
                                      <bodyText>
                                        <div class="buffer">
                                          <div class="border">
                                            <div class="body">As you experiment with the balloons, look for patterns.</div>
                                          </div>
                                        </div>
                                        <img src="http://udl.concord.org/share/shared-images/roxy-anim.gif" height="150" width="200" />
                                      </bodyText>
                                    </OTUDLCompoundDoc>
                                  </statement>
                                </OTUDLCoachStatement>
                                <OTUDLCoachStatement>
                                  <coach>
                                    <object refid="${character_strategic}" />
                                  </coach>
                                  <statement>
                                    <OTUDLCompoundDoc>
                                      <bodyText>
                                        <div class="buffer">
                                          <div class="border">
                                            <div class="body">Think about the major events of the story.  In Eduardo's words, the balloons were the best part of the party.</div>
                                          </div>
                                        </div>
                                      </bodyText>
                                    </OTUDLCompoundDoc>
                                  </statement>
                                </OTUDLCoachStatement>
                              </coachStatements>
                            </OTUDLCompoundDoc>
                            <OTUDLCompoundDoc name="%5d742e70-781a-11dc-a365-9554a3cf1337" showDefinitions="true">
                              <bodyText>
                                <div class="buffer">
                                  <div class="border">
                                    <div class="body">
                                      <table width="100%">
                                        <tr>
                                          <td>
                                            <div class="subtitle">Engage</div>
                                          </td>
                                          <td align="right">
                                            <div class="page-number">2</div>
                                          </td>
                                        </tr>
                                      </table>
                                      <div style="border-color: black; border-top-width: 1px; margin-right: 5px; margin-left: 5px; border-style: solid; margin-bottom: 5px; border-right-width: 1px; padding-left: 2px; padding-bottom: 2px; border-left-width: 1px; padding-top: 2px; padding-right: 2px; margin-top: 5px; border-bottom-width: 1px">
                                        <object refid="ae6840f8-6497-11dd-9c53-89770638a639" />
                                        <br />
                                        <object viewid="29dc5ef0-c453-11dc-95ff-0800200c9a66" refid="ae6840f9-6497-11dd-9c53-89770638a639" />
                                      </div>
                                    </div>
                                  </div>
                                </div>
                              </bodyText>
                              <documentRefs>
                                <OTCardContainer id="ae6840f8-6497-11dd-9c53-89770638a639">
                                  <cards>
                                    <OTUDLCompoundDoc id="ae68680a-6497-11dd-9c53-89770638a639">
                                      <bodyText>
                                        Electric charges are everywhere. They are very small - even smaller 
          than atoms! You can't see them. There are two kinds of electric 
          charge, called positive
                                        <img src="http://udl.concord.org/artwork/elect_34/red_postive_charge/el_34_red_positive_charge.png" />
                                        and negative.
                                        <img src="http://udl.concord.org/artwork/elect_34/black_negative_charge/el_34_black_negative_charge.png" />
                                      </bodyText>
                                    </OTUDLCompoundDoc>
                                    <OTUDLCompoundDoc id="c377562b-6497-11dd-9c53-89770638a639" name="%c377562b-6497-11dd-9c53-89770638a639" showEditBar="true">
                                      <bodyText>
                                        Every object has a huge number of both kinds of charge. If the number of 
    positive and negative charges is equal, they cancel each other out and the 
    object is electrically neutral.
                                        <br />
                                        <br />
                                        <center>
                                          <img src="http://udl.concord.org/artwork/elect_34/charges/el_34_11+11-charges.jpg" />
                                        </center>
                                      </bodyText>
                                    </OTUDLCompoundDoc>
                                    <OTUDLCompoundDoc id="cefc18fc-6497-11dd-9c53-89770638a639" name="%cefc18fc-6497-11dd-9c53-89770638a639" showEditBar="true">
                                      <bodyText>
                                        But an object can have slightly more positive than negative charges. Then 
    it becomes positively charged.
                                        <br />
                                        <br />
                                        <center>
                                          <img src="http://udl.concord.org/artwork/elect_34/charges/el_34_11+9-charges.jpg" />
                                        </center>
                                      </bodyText>
                                    </OTUDLCompoundDoc>
                                    <OTUDLCompoundDoc id="d7590eed-6497-11dd-9c53-89770638a639" name="%d7590eed-6497-11dd-9c53-89770638a639" showEditBar="true">
                                      <bodyText>
                                        Or it can have slightly more negative than positive charges, and then it 
    becomes negatively charged.
                                        <br />
                                        <br />
                                        <center>
                                          <img src="http://udl.concord.org/artwork/elect_34/charges/el_34_9+11-charges.jpg" />
                                          <br />
                                          <br />
                                        </center>
                                      </bodyText>
                                    </OTUDLCompoundDoc>
                                    <OTUDLCompoundDoc id="5645e2fe-6498-11dd-9c53-89770638a639" name="%5645e2fe-6498-11dd-9c53-89770638a639" showEditBar="true">
                                      <bodyText>You can't create electric charges out of nothing. But when you rub a 
    balloon on wool, negative charges move from the wool to the balloon. The 
    balloon becomes negatively charged and the wool becomes positively 
    charged. Then you can observe the force between them.</bodyText>
                                    </OTUDLCompoundDoc>
                                  </cards>
                                  <currentCard>
                                    <object refid="ae68680a-6497-11dd-9c53-89770638a639" />
                                  </currentCard>
                                </OTCardContainer>
                                <object refid="29dc5ef0-c453-11dc-95ff-0800200c9a66" />
                                <OTUDLMenu id="ae6840f9-6497-11dd-9c53-89770638a639" showBorder="false">
                                  <cardContainer>
                                    <object refid="ae6840f8-6497-11dd-9c53-89770638a639" />
                                  </cardContainer>
                                </OTUDLMenu>
                              </documentRefs>
                            </OTUDLCompoundDoc>
                            <OTUDLCompoundDoc name="%f82b1d82-781a-11dc-a365-9554a3cf1337" showDefinitions="true">
                              <bodyText>
                                <div class="buffer">
                                  <div class="border">
                                    <div class="body">
                                      <table width="100%">
                                        <tr>
                                          <td>
                                            <div class="subtitle" />
                                          </td>
                                          <td align="right">
                                            <div class="page-number">3</div>
                                          </td>
                                        </tr>
                                      </table>
                                      <table width="100%">
                                        <tr>
                                          <td width="30%">
                                            <br />
                                            <div style="padding-bottom: 2px; margin-left: 5px; margin-bottom: 5px; margin-right: 5px; padding-top: 2px; padding-right: 2px; border-style: solid; border-right-width: 1px; border-color: black; padding-left: 2px; border-left-width: 1px; border-top-width: 1px; margin-top: 5px; border-bottom-width: 1px">
                                              <object refid="0ebbb7a7-6499-11dd-9c53-89770638a639" />
                                              <br />
                                              <object viewid="29dc5ef0-c453-11dc-95ff-0800200c9a66" refid="0ebbb7a8-6499-11dd-9c53-89770638a639" />
                                            </div>
                                          </td>
                                          <td width="70%">
                                            <object refid="81c20131-781a-11dc-a365-9554a3cf1337" />
                                          </td>
                                        </tr>
                                      </table>
                                    </div>
                                  </div>
                                </div>
                              </bodyText>
                              <documentRefs>
                                <OTCardContainer id="0ebbb7a7-6499-11dd-9c53-89770638a639">
                                  <cards>
                                    <OTUDLCompoundDoc id="0ebbb7a9-6499-11dd-9c53-89770638a639">
                                      <bodyText>
                                        Here are a balloon and a wool sweater. Using the Stamp tool, put 5
                                        <img src="http://udl.concord.org/artwork/elect_34/red_postive_charge/el_34_red_positive_charge.png" />
                                        signs and 5
                                        <img src="http://udl.concord.org/artwork/elect_34/black_negative_charge/el_34_black_negative_charge.png" />
                                        signs on the balloon. Also put 5
                                        <img src="http://udl.concord.org/artwork/elect_34/red_postive_charge/el_34_red_positive_charge.png" />
                                        signs and 5
                                        <img src="http://udl.concord.org/artwork/elect_34/black_negative_charge/el_34_black_negative_charge.png" />
                                        signs on the wool sweater.
                                      </bodyText>
                                    </OTUDLCompoundDoc>
                                    <OTUDLCompoundDoc id="55da530c-649c-11dd-a494-4fdf7e1f20f7" name="%55da530c-649c-11dd-a494-4fdf7e1f20f7" showEditBar="true">
                                      <documentRefs>
                                        <OTUDLQuestion id="907c6427-c842-11dc-8b07-cd90cfa5dd97" name="Question" showConvertButton="true">
                                          <prompt>
                                            <OTUDLCompoundDoc>
                                              <bodyText>Is the balloon electrically charged, or is it electrically neutral?</bodyText>
                                            </OTUDLCompoundDoc>
                                          </prompt>
                                          <input>
                                            <OTChoice name="Multiple choice" alignContent="0">
                                              <choices>
                                                <OTCompoundDoc name="electrically neutral">
                                                  <bodyText>electrically neutral</bodyText>
                                                </OTCompoundDoc>
                                                <OTCompoundDoc name="positively charged">
                                                  <bodyText>positively charged</bodyText>
                                                </OTCompoundDoc>
                                                <OTCompoundDoc name="negatively charged">
                                                  <bodyText>negatively charged</bodyText>
                                                </OTCompoundDoc>
                                              </choices>
                                            </OTChoice>
                                          </input>
                                        </OTUDLQuestion>
                                      </documentRefs>
                                      <bodyText>
                                        <object refid="907c6427-c842-11dc-8b07-cd90cfa5dd97" />
                                      </bodyText>
                                    </OTUDLCompoundDoc>
                                    <OTUDLCompoundDoc id="57f9d7ad-649c-11dd-a494-4fdf7e1f20f7" name="%57f9d7ad-649c-11dd-a494-4fdf7e1f20f7" showEditBar="true">
                                      <documentRefs>
                                        <OTUDLLeveledQuestions id="cb7f2dbd-be21-11dc-b1c9-797f3b144f00" name="Leveled questions" timesClicked="0">
                                          <questions>
