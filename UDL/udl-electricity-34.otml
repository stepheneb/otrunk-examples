                            <OTUDLQuestion id="686fe0dd-b965-11dc-9c96-c9838ca6fb43" name="Question">
                              <prompt>
                                <OTUDLCompoundDoc>
                                  <bodyText>Some electrical devices use many batteries in a row. Why do you think they do this?</bodyText>
                                </OTUDLCompoundDoc>
                              </prompt>
                              <input>
                                <OTText />
                              </input>
                              <correctAnswer>
                                <OTUDLCompoundDoc>
                                  <bodyText>Many batteries together can produce a larger voltage. Also they last  longer because there is energy available from each battery.</bodyText>
                                </OTUDLCompoundDoc>
                              </correctAnswer>
                            </OTUDLQuestion>
                            <OTUDLQuestion id="ac3ee29a-b965-11dc-9c96-c9838ca6fb43" name="Question">
                              <prompt>
                                <OTUDLCompoundDoc>
                                  <bodyText>Suppose you made a row of 4 batteries, all pointed the same way, and each one was 2.0 volts. What do you think the total voltage would be?</bodyText>
                                </OTUDLCompoundDoc>
                              </prompt>
                              <input>
                                <OTText />
                              </input>
                              <correctAnswer>
                                <OTUDLCompoundDoc>
                                  <bodyText>8 Volts</bodyText>
                                </OTUDLCompoundDoc>
                              </correctAnswer>
                            </OTUDLQuestion>
