                            <OTQuestion id="eb0e6e5b-e0c6-11dc-8b8f-579135bdfd8c" name="Question" showConvertButton="true">
                              <prompt>
                                <OTUDLCompoundDoc>
                                  <bodyText>
                                    Draw a summary of Chapter 5.
                                    <br />
                                    <br />
                                  </bodyText>
                                </OTUDLCompoundDoc>
                              </prompt>
                              <input>
                                <OTDrawingTool name="Drawing tool" />
