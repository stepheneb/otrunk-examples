                                    </OTUDLCompoundDoc>
                                    <OTUDLCompoundDoc id="428bde46-64ba-11dd-81ae-a16f389bc712" name="%428bde46-64ba-11dd-81ae-a16f389bc712" showEditBar="true">
                                      <documentRefs>
                                        <OTUDLQuestion id="46032abc-64ba-11dd-81ae-a16f389bc712" name="Question" showConvertButton="true">
                                          <prompt>
                                            <OTCompoundDoc>
                                              <bodyText>Electricity produces sound.</bodyText>
                                            </OTCompoundDoc>
