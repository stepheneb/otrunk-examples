                    <OTUDLSection local_id="section_5" name="Volts and lights" isComputerBased="true" isIntroduction="false" isMath="false" isPosttest="false" isPretest="false" isReading="false" isWrapup="false" description="How do light bulbs work?">
                      <content>
                        <OTCardContainer id="4e5e0956-cdc1-11dc-bd2b-dd05e7961664" contentsMayScroll="true" loadViewOnlyOnDemand="true">
                          <cards>
                            <OTUDLCompoundDoc id="4e5e0955-cdc1-11dc-bd2b-dd05e7961664" showDefinitions="true">
                              <bodyText>
                                <div class="buffer">
                                  <div class="border">
                                    <div class="body">
                                      <table width="100%">
                                        <tr>
                                          <td>
                                            <div class="subtitle" />
