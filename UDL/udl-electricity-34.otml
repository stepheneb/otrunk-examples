                        <OTUDLCompoundDoc name="%93e3c518-781b-11dc-a365-9554a3cf1337">
                          <bodyText>
                            <div class="buffer">
                              <div class="border">
                                <div class="body">
                                  <div class="subtitle-continued">Explain 2</div>
                                  Based on your observations, answer these questions with
                                  <b>true</b>
                                  or
                                  <b>false.</b>
                                  <p />
                                  Similar charges attract (pull toward) each other.
                                  <br />
                                  <object refid="6b4ce18e-781c-11dc-a365-9554a3cf1337" />
                                  <p />
                                  Similar charges repel (push away from) each other.
                                  <br />
                                  <object refid="8eeb8792-781c-11dc-a365-9554a3cf1337" />
                                  <p />
                                  Positive charges attract negative charges.
                                  <br />
                                  <object refid="c66044fd-781b-11dc-a365-9554a3cf1337" />
                                  <p />
                                  Positive charges repel negative charges.
                                  <br />
                                  <object refid="b9e53b49-781b-11dc-a365-9554a3cf1337" />
                                </div>
                              </div>
                            </div>
                          </bodyText>
                          <documentRefs>
                            <OTChoice id="6b4ce18e-781c-11dc-a365-9554a3cf1337" name="Choices" alignContent="0">
                              <choices>
                                <OTCompoundDoc name="true">
                                  <bodyText>true</bodyText>
                                </OTCompoundDoc>
                                <OTCompoundDoc name="false">
                                  <bodyText>false</bodyText>
                                </OTCompoundDoc>
                              </choices>
                            </OTChoice>
                            <OTChoice id="8eeb8792-781c-11dc-a365-9554a3cf1337" name="Choices" alignContent="0">
                              <choices>
                                <OTCompoundDoc name="true">
                                  <bodyText>true</bodyText>
                                </OTCompoundDoc>
                                <OTCompoundDoc name="false">
                                  <bodyText>false</bodyText>
                                </OTCompoundDoc>
                              </choices>
                            </OTChoice>
                            <OTChoice id="c66044fd-781b-11dc-a365-9554a3cf1337" name="Choices" alignContent="0">
                              <choices>
                                <OTCompoundDoc name="true">
                                  <bodyText>true</bodyText>
                                </OTCompoundDoc>
                                <OTCompoundDoc name="false">
                                  <bodyText>false</bodyText>
                                </OTCompoundDoc>
                              </choices>
                            </OTChoice>
                            <OTChoice id="b9e53b49-781b-11dc-a365-9554a3cf1337" name="Choices" alignContent="0">
                              <choices>
                                <OTCompoundDoc name="true">
                                  <bodyText>true</bodyText>
                                </OTCompoundDoc>
                                <OTCompoundDoc name="false">
                                  <bodyText>false</bodyText>
                                </OTCompoundDoc>
                              </choices>
                            </OTChoice>
                          </documentRefs>
                          <coachStatements>
                            <OTUDLCoachStatement>
                              <coach>
                                <object refid="${character_affective}" />
                              </coach>
                              <statement>
                                <OTUDLCompoundDoc>
                                  <bodyText>
                                    <div class="buffer">
                                      <div class="border">
                                        <div class="body">Good scientists make careful observations.</div>
                                      </div>
                                    </div>
                                  </bodyText>
                                </OTUDLCompoundDoc>
                              </statement>
                            </OTUDLCoachStatement>
                            <OTUDLCoachStatement>
                              <coach>
                                <object refid="${character_strategic}" />
                              </coach>
                              <statement>
                                <OTUDLCompoundDoc>
                                  <bodyText>
                                    <div class="buffer">
                                      <div class="border">
                                        <div class="body">Some negative charges moved from the wool to the balloon.  You need to know how charges react to one another.</div>
                                      </div>
                                    </div>
                                  </bodyText>
                                </OTUDLCompoundDoc>
                              </statement>
                            </OTUDLCoachStatement>
                            <OTUDLCoachStatement>
                              <coach>
                                <object refid="${character_recognition}" />
                              </coach>
                              <statement>
                                <OTUDLCompoundDoc>
                                  <bodyText>
                                    <div class="buffer">
                                      <div class="border">
                                        <div class="body">Think about the experiment you did with the balloons. Your observations should help you answer these questions.</div>
                                      </div>
                                    </div>
                                  </bodyText>
                                </OTUDLCompoundDoc>
