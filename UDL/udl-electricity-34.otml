                                              <prompt>
                                                <OTUDLCompoundDoc>
                                                  <bodyText>9. If any one dot on the string is moved, what happens to the other dots?</bodyText>
                                                </OTUDLCompoundDoc>
                                              </prompt>
                                              <input>
                                                <OTText />
                                              </input>
