                                              <prompt>
                                                <OTUDLCompoundDoc>
                                                  <bodyText>Why do you think so?</bodyText>
                                                </OTUDLCompoundDoc>
                                              </prompt>
                                              <input>
                                                <OTText />
                                              </input>
                                            </OTUDLQuestion>
                                            <OTUDLQuestion name="L4" showConvertButton="false" showScaffolding="true">
                                              <prompt>
                                                <OTUDLCompoundDoc>
                                                  <bodyText>Why do you think so?</bodyText>
                                                </OTUDLCompoundDoc>
                                              </prompt>
                                              <input>
                                                <OTText />
                                              </input>
                                              <scaffolding>
                                                <OTUDLCompoundDoc>
                                                  <bodyText>Think about if the number of positive and negative charges equal.</bodyText>
                                                </OTUDLCompoundDoc>
                                              </scaffolding>
                                            </OTUDLQuestion>
                                            <OTUDLQuestion name="L3" showConvertButton="false" showScaffolding="true">
                                              <prompt>
                                                <OTUDLCompoundDoc>
                                                  <bodyText>Why do you think so?</bodyText>
                                                </OTUDLCompoundDoc>
                                              </prompt>
                                              <input>
                                                <OTText />
                                              </input>
                                              <scaffolding>
                                                <OTUDLCompoundDoc>
                                                  <bodyText>Because the number of positive and negative charges are the same, the object is electrically _____________.</bodyText>
                                                </OTUDLCompoundDoc>
                                              </scaffolding>
                                            </OTUDLQuestion>
                                            <OTUDLQuestion name="L2" showConvertButton="false" showScaffolding="true">
                                              <prompt>
                                                <OTUDLCompoundDoc>
                                                  <bodyText>Why do you think so?</bodyText>
                                                </OTUDLCompoundDoc>
                                              </prompt>
                                              <input>
                                                <OTText />
                                              </input>
                                            </OTUDLQuestion>
                                            <OTUDLQuestion name="L1" showConvertButton="false" showScaffolding="true">
                                              <prompt>
                                                <OTUDLCompoundDoc>
                                                  <bodyText>Why do you think so?</bodyText>
                                                </OTUDLCompoundDoc>
                                              </prompt>
                                              <input>
                                                <OTText />
                                              </input>
                                              <scaffolding>
                                                <OTUDLCompoundDoc>
                                                  <bodyText>Positive and negative charges cancel one another out.</bodyText>
                                                </OTUDLCompoundDoc>
                                              </scaffolding>
                                            </OTUDLQuestion>
                                          </questions>
                                        </OTUDLLeveledQuestions>
                                      </documentRefs>
                                      <bodyText>
                                        <object refid="cb7f2dbd-be21-11dc-b1c9-797f3b144f00" />
                                      </bodyText>
                                    </OTUDLCompoundDoc>
                                    <OTUDLCompoundDoc id="599d758e-649c-11dd-a494-4fdf7e1f20f7" name="%599d758e-649c-11dd-a494-4fdf7e1f20f7" showEditBar="true">
                                      <documentRefs>
                                        <OTUDLQuestion id="da5b3aab-c846-11dc-b8b6-397a07a82a77" name="Question" showConvertButton="true">
                                          <prompt>
                                            <OTUDLCompoundDoc>
                                              <bodyText>Now is the balloon electrically charged, or is it electrically neutral?</bodyText>
                                            </OTUDLCompoundDoc>
                                          </prompt>
                                          <input>
                                            <OTText />
                                          </input>
                                        </OTUDLQuestion>
                                      </documentRefs>
                                      <bodyText>
                                        Now drag some negative charges from the wool sweater to the balloon. This 
    is what happens when you rub them together.
                                        <br />
                                        <br />
                                        <object refid="da5b3aab-c846-11dc-b8b6-397a07a82a77" />
                                      </bodyText>
                                    </OTUDLCompoundDoc>
                                    <OTUDLCompoundDoc id="5ab3043f-649c-11dd-a494-4fdf7e1f20f7" name="%5ab3043f-649c-11dd-a494-4fdf7e1f20f7" showEditBar="true">
                                      <documentRefs>
                                        <OTUDLQuestion id="f5e650d6-c846-11dc-b8b6-397a07a82a77" name="Question" showConvertButton="true">
                                          <prompt>
                                            <OTUDLCompoundDoc>
                                              <bodyText>Why do you think so?</bodyText>
                                            </OTUDLCompoundDoc>
                                          </prompt>
                                          <input>
                                            <OTText />
                                          </input>
                                        </OTUDLQuestion>
                                      </documentRefs>
                                      <bodyText>
                                        <object refid="f5e650d6-c846-11dc-b8b6-397a07a82a77" />
                                      </bodyText>
                                    </OTUDLCompoundDoc>
                                  </cards>
                                  <currentCard>
                                    <object refid="0ebbb7a9-6499-11dd-9c53-89770638a639" />
                                  </currentCard>
