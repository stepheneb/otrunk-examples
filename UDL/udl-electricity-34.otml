                            <OTUDLQuestion id="c71f0f15-ca09-11dc-a33a-97fd86165789" name="Question" showConvertButton="true">
                              <prompt>
                                <OTUDLCompoundDoc>
                                  <bodyText>What things in your house run on electrical energy? Discuss this question with members of your group and make a list. Be prepared to discuss your ideas with the class.</bodyText>
                                </OTUDLCompoundDoc>
                              </prompt>
                              <input>
                                <OTText text="" />
                              </input>
                              <correctAnswer>
                                <OTCompoundDoc>
                                  <bodyText>Some examples are clock, computer, lamp, stereo, TV, vacuum cleaner, toaster, hair dryer, washing machine, garbage disposal, dishwasher, flashlight, portable CD player.</bodyText>
                                </OTCompoundDoc>
                              </correctAnswer>
                            </OTUDLQuestion>
