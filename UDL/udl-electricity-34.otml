                    <OTUDLSection local_id="section_6" name="String circuit" isComputerBased="true" isIntroduction="false" isMath="false" isPosttest="false" isPretest="false" isReading="false" isWrapup="false" description="How does electricity move through a wire?">
                      <content>
                        <OTCardContainer id="236e6d61-775f-11dc-88e0-f91e4efaa30a" contentsMayScroll="true" loadViewOnlyOnDemand="true">
                          <cards>
                            <OTUDLCompoundDoc id="236e6d60-775f-11dc-88e0-f91e4efaa30a" name="%236e6d60-775f-11dc-88e0-f91e4efaa30a" showDefinitions="true">
                              <bodyText>
                                <div class="buffer">
                                  <div class="border">
                                    <div class="body">
