                            <OTUDLQuestion id="a96b1f4e-8fe7-11dd-91a5-717f44997088" name="Question" showConvertButton="true">
                              <prompt>
                                <OTCompoundDoc>
                                  <bodyText>1. Does subtracting (removing) a positive number give the same result as adding a negative number? That is, is it true that 5 - 2 = 5 + (-2)?</bodyText>
                                </OTCompoundDoc>
                              </prompt>
                              <input>
                                <OTText />
                              </input>
                            </OTUDLQuestion>
