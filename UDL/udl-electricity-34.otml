                                          <script>
                                            <OTJavascript>
                                              <script>importClass(Packages.javax.swing.JOptionPane);

function clicked(){
   highlight(2.5, 6)
  var xvalue = getLabelX()
  showMessage(xvalue) 
  if (xvalue &gt;  2.5 &amp;&amp; xvalue &lt; 6){
       showMessage ("Correct!" )
       correct.setValue(true)
  } 
  else {
   showMessage ("Look for the lowest voltage that is greater than zero.")
   incorrect()
  }
  
}


function showMessage(message){
    JOptionPane.showMessageDialog(null, message);
}

function highlight(start, end){
    smart.showRegionLabel(start, end);
}

function removeHighlight(){
    smart.removeAllRegionLabels();
}

function getLabelX(){
    return getLastValidLabel().getXData();
}

function getLabelY(){
    return getLastValidLabel().getYData();
}

function getLastValidLabel(){
    var labels = graph.getLabels();
    
   	if (labels.size() == 0){
   	   return null;
   	}
   	
   	for (i=labels.size()-1; i&gt;0; i--){
   	   var label = labels.get(i);
   	   if (label.getX() &lt; 100000){
   	      return label;
   	   }
   	}
   	return null;
}

function incorrect(){
  times_incorrect.setValue(times_incorrect.getValue() + 1)
  correct.setValue(false)
}</script>
