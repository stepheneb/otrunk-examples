                                          <prompt>
                                            <OTUDLCompoundDoc>
                                              <bodyText>1. Use the mouse to drag the balloon up and down over the wool sweater. What happens to the charges?</bodyText>
                                            </OTUDLCompoundDoc>
                                          </prompt>
                                          <input>
                                            <OTText />
                                          </input>
                                          <correctAnswer>
                                            <OTUDLCompoundDoc>
                                              <bodyText>Negative charges move from the sweater to the balloon.</bodyText>
                                            </OTUDLCompoundDoc>
                                          </correctAnswer>
                                        </OTUDLQuestion>
                                      </documentRefs>
                                      <bodyText>
                                        <object refid="a4bbb3b8-ccdf-11dc-b389-a1ad3851fa89" />
                                      </bodyText>
                                    </OTUDLCompoundDoc>
                                    <OTUDLCompoundDoc id="36517a74-649f-11dd-a494-4fdf7e1f20f7" name="%36517a74-649f-11dd-a494-4fdf7e1f20f7" showEditBar="true">
                                      <documentRefs>
