                                    <object refid="${character_affective}" />
                                  </coach>
                                  <statement>
                                    <OTUDLCompoundDoc>
                                      <bodyText>
                                        <div class="buffer">
                                          <div class="border">
                                            <div class="body">Now that we are at the end, it's a great time to summarize the story.</div>
                                          </div>
                                        </div>
                                        <img src="http://udl.concord.org/share/shared-images/blah-anim.gif" height="150" width="200" />
                                      </bodyText>
                                    </OTUDLCompoundDoc>
                                  </statement>
                                </OTUDLCoachStatement>
                                <OTUDLCoachStatement>
                                  <coach>
                                    <object refid="${character_recognition}" />
                                  </coach>
                                  <statement>
                                    <OTUDLCompoundDoc>
                                      <bodyText>
                                        <div class="buffer">
                                          <div class="border">
                                            <div class="body">During a birthday party, the children learned about electrical charges by separating charges on balloons and seeing them with a special magnifying glass.</div>
                                          </div>
                                        </div>
                                        <img src="http://udl.concord.org/share/shared-images/roxy-anim.gif" height="150" width="200" />
                                      </bodyText>
                                    </OTUDLCompoundDoc>
                                  </statement>
                                </OTUDLCoachStatement>
                                <OTUDLCoachStatement>
                                  <coach>
                                    <object refid="${character_strategic}" />
                                  </coach>
                                  <statement>
                                    <OTUDLCompoundDoc>
                                      <bodyText>
                                        <div class="buffer">
                                          <div class="border">
                                            <div class="body">Think about the major events of the story.  In Chen's words, the balloons were the best part of the party.</div>
                                          </div>
                                        </div>
                                      </bodyText>
                                    </OTUDLCompoundDoc>
                                  </statement>
                                </OTUDLCoachStatement>
                              </coachStatements>
                            </OTUDLCompoundDoc>
                          </cards>
