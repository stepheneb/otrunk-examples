                                    <OTUDLCompoundDoc id="a384d589-64b6-11dd-81ae-a16f389bc712" name="%a384d589-64b6-11dd-81ae-a16f389bc712" showEditBar="true">
                                      <documentRefs>
                                        <OTUDLQuestion id="554c68fc-cdc3-11dc-bd2b-dd05e7961664" name="Question" showConvertButton="true">
                                          <prompt>
                                            <OTUDLCompoundDoc>
                                              <bodyText>What is the voltage if you don't crank at all?</bodyText>
                                            </OTUDLCompoundDoc>
                                          </prompt>
                                          <input>
                                            <OTText />
                                          </input>
                                          <correctAnswer>
                                            <OTUDLCompoundDoc>
                                              <bodyText>The voltage is zero.</bodyText>
                                            </OTUDLCompoundDoc>
                                          </correctAnswer>
                                        </OTUDLQuestion>
                                      </documentRefs>
                                      <bodyText>
                                        8. Crank the Genecon and watch the graph.
                                        <br />
                                        <object refid="554c68fc-cdc3-11dc-bd2b-dd05e7961664" />
                                      </bodyText>
                                    </OTUDLCompoundDoc>
                                    <OTUDLCompoundDoc id="d00ab11f-64b6-11dd-81ae-a16f389bc712" name="%d00ab11f-64b6-11dd-81ae-a16f389bc712" showEditBar="true">
                                      <documentRefs>
                                        <OTUDLQuestion id="1d340f57-cdc3-11dc-bd2b-dd05e7961664" name="Question" showConvertButton="true">
                                          <prompt>
                                            <OTUDLCompoundDoc>
                                              <bodyText>9. What happens to the voltage if you crank slower or faster?</bodyText>
                                            </OTUDLCompoundDoc>
                                          </prompt>
                                          <input>
                                            <OTText />
                                          </input>
                                          <correctAnswer>
                                            <OTUDLCompoundDoc>
                                              <bodyText>If you crank faster, the voltage increases. If you crank slower, the voltage decreases.</bodyText>
                                            </OTUDLCompoundDoc>
                                          </correctAnswer>
                                        </OTUDLQuestion>
                                      </documentRefs>
                                      <bodyText>
                                        <object refid="1d340f57-cdc3-11dc-bd2b-dd05e7961664" />
                                      </bodyText>
