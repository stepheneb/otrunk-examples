                                <OTDataCollector id="37cafb27-6d55-11dd-ac9a-cb602d69c6ea" name="crank_voltage_example" title="light a bulb: example" autoScaleEnabled="true">
                                  <source>
                                    <OTDataGraphable id="37cafb27-6d55-11dd-ac9a-cb602d69c6ea/source" name="Sensor" connectPoints="true" yColumn="1" drawMarks="false" color="255" xColumn="0" lineWidth="2.0">
                                      <dataProducer>
