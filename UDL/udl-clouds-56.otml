                              </statement>
                            </OTUDLCoachStatement>
                            <OTUDLCoachStatement>
                              <coach>
                                <object refid="${character_recognition}"/>
                              </coach>
                              <statement>
                                <OTUDLCompoundDoc>
                                  <bodyText>
                                    <div class="buffer">
                                      <div class="border">
                                        <div class="body">Marc and Natasha look happy to be flying.</div>
                                      </div>
                                    </div>
                                    <img src="jres:///org/concord/otrunk/udl/document/roxy-anim.gif" height="150" width="200"/>
                                  </bodyText>
                                </OTUDLCompoundDoc>
                              </statement>
                            </OTUDLCoachStatement>
