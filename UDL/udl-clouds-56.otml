                    <OTUDLSection local_id="section_7" name="Humidity" description="What does humidity have to do with clouds?" isComputerBased="true" isIntroduction="false" isMath="false" isPosttest="false" isPretest="false" isReading="false" isWrapup="false">
                      <content>
                        <OTCardContainer id="5f56ae60-768b-11dc-a568-7984efa6d7c4" contentsMayScroll="true" loadViewOnlyOnDemand="true" closeViewOnExit="false">
                          <cards>
                            <OTUDLCompoundDoc id="cb42ac22-bee3-11dc-8394-53bb1076bc90" name="%cb41e8d1-bee3-11dc-8394-53bb1076bc90" showDefinitions="true">
                              <bodyText>
                                <div class="buffer">
                                  <div class="border">
                                    <div class="body">
