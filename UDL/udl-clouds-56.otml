                              <prompt>
                                <OTCompoundDoc>
                                  <bodyText>How do you know water is in the air? Give an example from your own experience.</bodyText>
                                </OTCompoundDoc>
                              </prompt>
                              <input>
                                <OTText />
                              </input>
                            </OTUDLQuestion>
                          </documentRefs>
                        </OTUDLCompoundDoc>
