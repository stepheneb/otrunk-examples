                                <OTText />
                              </input>
                            </OTUDLQuestion>
                            <OTUDLQuestion id="92245d38-5e3f-11dd-9fb3-4d04373ebcb1" name="Question" showConvertButton="true">
                              <prompt>
                                <OTCompoundDoc>
                                  <bodyText>Read through these possible answers. Which ones are like your ideas?</bodyText>
                                </OTCompoundDoc>
                              </prompt>
                              <input>
                                <OTChoiceMultiple name="Multiple choice (check boxes)" alignContent="0">
                                  <choices>
                                    <OTCompoundDoc name="Water droplets form around dust particles.">
                                      <bodyText>Water droplets form around dust particles.</bodyText>
                                    </OTCompoundDoc>
                                    <OTCompoundDoc name="A cloud is made of fog or mist.">
                                      <bodyText>A cloud is made of fog or mist.</bodyText>
                                    </OTCompoundDoc>
                                    <OTCompoundDoc name="A cloud is made when water in the air condenses into water droplets.">
                                      <bodyText>A cloud is made when water in the air condenses into water droplets.</bodyText>
                                    </OTCompoundDoc>
                                    <OTCompoundDoc name="Water droplets are made when water molecules in the air clump together.">
                                      <bodyText>Water droplets are made when water molecules in the air clump together.</bodyText>
                                    </OTCompoundDoc>
                                    <OTCompoundDoc name="A cloud is made when the air becomes cold and the water vapor condenses.">
                                      <bodyText>A cloud is made when the air becomes cold and the water vapor condenses.</bodyText>
                                    </OTCompoundDoc>
                                    <OTCompoundDoc name="A cloud can be made of water droplets or ice crystals.">
                                      <bodyText>A cloud can be made of water droplets or ice crystals.</bodyText>
                                    </OTCompoundDoc>
                                  </choices>
                                </OTChoiceMultiple>
                              </input>
                            </OTUDLQuestion>
                          </documentRefs>
                        </OTUDLCompoundDoc>
                        <OTUDLCompoundDoc id="7bea42fe-1af6-11dd-a75b-55af5827ccdf" name="%7bea42fd-1af6-11dd-a75b-55af5827ccdf" showDefinitions="true">
                          <documentRefs>
                            <OTUDLQuestion id="33524a7d-2cfa-11dd-9067-dd2c145538bd" name="Question" showConvertButton="true">
                              <prompt>
                                <OTCompoundDoc>
                                  <bodyText>On a warm day, when the water in a puddle disappears, what happens to the water molecules in the puddle?</bodyText>
                                </OTCompoundDoc>
                              </prompt>
                              <input>
                                <OTText />
                              </input>
                              <correctAnswer>
                                <OTCompoundDoc>
                                  <bodyText>When sun heats the water molecules, they go up into the air. This is called evaporation. The water molecules zoom around in the air banging into each other and other gas molecules. This water vapor is invisible.</bodyText>
                                </OTCompoundDoc>
                              </correctAnswer>
                            </OTUDLQuestion>
                          </documentRefs>
