                                <OTUDLCompoundDoc>
                                  <bodyText>
                                    <div class="buffer">
                                      <div class="border">
                                        <div class="body">Marc described a cloud that looks like an elephant.  I have seen a 
          cloud that looks like a dragon.</div>
                                      </div>
                                    </div>
                                    <img width="200" height="150" src="jres:///org/concord/otrunk/udl/document/roxy-anim.gif"/>
