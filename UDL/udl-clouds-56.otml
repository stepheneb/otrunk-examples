                            </OTUDLCoachStatement>
                            <OTUDLCoachStatement timesClicked="4">
                              <coach>
                                <object refid="${character_strategic}" />
                              </coach>
                              <statement>
                                <OTUDLCompoundDoc>
                                  <bodyText>
                                    <div class="buffer">
                                      <div class="border">
                                        <div class="body">Read the title and look at the pictures for clues about the story.</div>
                                      </div>
                                    </div>
                                    <img src="http://udl.concord.org/share/shared-images/clunk-anim.gif" height="150" width="200" />
                                  </bodyText>
                                </OTUDLCompoundDoc>
                              </statement>
                            </OTUDLCoachStatement>
                            <OTUDLCoachStatement timesClicked="4">
                              <coach>
                                <object refid="${character_recognition}" />
                              </coach>
                              <statement>
                                <OTUDLCompoundDoc>
                                  <bodyText>
                                    <div class="buffer">
                                      <div class="border">
                                        <div class="body">I predict the children will learn about clouds.</div>
                                      </div>
                                    </div>
                                    <img src="http://udl.concord.org/share/shared-images/roxy-anim.gif" height="150" width="200" />
                                  </bodyText>
                                </OTUDLCompoundDoc>
                              </statement>
