                                              </prompt>
                                              <input>
                                                <OTText/>
                                              </input>
                                              <correctAnswer>
                                                <OTCompoundDoc>
                                                  <bodyText>oceans and seas, icecaps and glaciers, groundwater, lakes, atmosphere, rivers</bodyText>
                                                </OTCompoundDoc>
                                              </correctAnswer>
                                            </OTUDLQuestion>
                                            <OTUDLQuestion name="L4" showConvertButton="false" showScaffolding="true">
