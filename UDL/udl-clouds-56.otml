                                <OTUDLCompoundDoc>
                                  <bodyText>
                                    <div class="buffer">
                                      <div class="border">
                                        <div class="body">Let's make a connection. What do clouds look like to you?</div>
                                      </div>
                                    </div>
                                    <img width="200" height="150" src="jres:///org/concord/otrunk/udl/document/blah-anim.gif"/>
                                  </bodyText>
                                </OTUDLCompoundDoc>
                              </statement>
                            </OTUDLCoachStatement>
                            <OTUDLCoachStatement>
                              <coach>
