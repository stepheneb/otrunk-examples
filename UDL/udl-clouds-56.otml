                          <documentRefs>
                            <OTUDLQuestion id="d96c30f9-00f0-11dd-942e-750cfc65e2cc" name="Question" showConvertButton="true">
                              <prompt>
                                <OTUDLCompoundDoc>
                                  <bodyText>What do you think clouds are made of?</bodyText>
                                </OTUDLCompoundDoc>
