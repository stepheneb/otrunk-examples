                                            <OTCompoundDoc name="Light and fluffy">
                                              <bodyText>Light and fluffy</bodyText>
                                            </OTCompoundDoc>
                                            <OTCompoundDoc name="Heavy and thick">
                                              <bodyText>Heavy and thick</bodyText>
                                            </OTCompoundDoc>
                                            <OTCompoundDoc name="Solid">
                                              <bodyText>Solid</bodyText>
                                            </OTCompoundDoc>
                                          </choices>
                                        </OTChoice>
                                      </documentRefs>
                                      <bodyText>
                                        The cloud is:
                                        <p />
                                        <object refid="5d4cdaf0-7693-11dc-8925-31e85bbc495c" />
                                      </bodyText>
                                    </OTUDLCompoundDoc>
                                  </input>
                                  <scaffolding>
                                    <OTUDLCompoundDoc>
                                      <bodyText>
                                        <div class="hint">Pick the best answer above to complete the sentence.</div>
                                      </bodyText>
                                    </OTUDLCompoundDoc>
                                  </scaffolding>
                                </OTQuestion>
                                <OTQuestion name="L1">
                                  <prompt>
                                    <OTUDLCompoundDoc>
                                      <bodyText>What is it like?</bodyText>
                                    </OTUDLCompoundDoc>
                                  </prompt>
                                  <input>
                                    <OTUDLCompoundDoc>
                                      <documentRefs>
                                        <OTText id="7a302e17-7693-11dc-8925-31e85bbc495c" name="Text field" />
                                      </documentRefs>
