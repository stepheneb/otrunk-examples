                            <object refid="${page-rule}" />
                          </menuRule>
                        </OTUDLMenu>
                        <object refid="${menu-horizontal-circle}" />
                      </documentRefs>
                      <bodyText>
                        <div align="center">
                          <table>
                            <tr>
                              <td>
                                <object refid="e9968743-71c3-11dc-9410-0f9c34fad6c2" viewid="${menu-horizontal-circle}" />
                              </td>
                            </tr>
                          </table>
                        </div>
                      </bodyText>
                    </OTUDLCompoundDoc>
                  </footer>
                  <north>
                    <object refid="e9946460-71c3-11dc-9410-0f9c34fad6c2" />
                  </north>
                  <center>
                    <object refid="e994645f-71c3-11dc-9410-0f9c34fad6c2" />
