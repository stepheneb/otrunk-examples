                        <OTUDLCompoundDoc id="6cebf869-71c2-11dc-9410-0f9c34fad6c2" name="6cebf869-71c2-11dc-9410-0f9c34fad6c2">
                          <bodyText>
                            <div class="buffer">
                              <div class="border">
                                <div class="body">
                                  <div class="subtitle">Engage</div>
                                  What do you think clouds are made of?
                                  <p />
                                  <img src="http://udl.concord.org/share/Clouds/Clouds46.jpg" />
                                  <p />
                                  In this activity, you will zoom into a cloud to see what it is made of.
                                </div>
                              </div>
                            </div>
                          </bodyText>
                          <coachStatements>
                            <OTUDLCoachStatement>
                              <coach>
                                <object refid="${character_affective}" />
                              </coach>
                              <statement>
                                <OTUDLCompoundDoc>
                                  <bodyText>
                                    <div class="buffer">
                                      <div class="border">
                                        <div class="body">Clouds are so cool!  I love to look at them.  Sometimes I pretend I can see different animals or other shapes.</div>
                                      </div>
                                    </div>
                                    <img src="http://confluence.concord.org/download/attachments/12353/blah-head-small.png" height="150" width="200" />
                                  </bodyText>
                                </OTUDLCompoundDoc>
                              </statement>
                            </OTUDLCoachStatement>
                          </coachStatements>
                        </OTUDLCompoundDoc>
                        <OTUDLCompoundDoc name="febbd1c0-71c5-11dc-9410-0f9c34fad6c2">
                          <bodyText>
                            <div class="buffer">
                              <div class="border">
                                <div class="body">
                                  <div class="subtitle">Explore</div>
                                  Take a look at this cloud.
                                  <p />
                                  <img src="http://udl.concord.org/share/Clouds/Clouds168.jpg" />
                                  <p />
                                  <object refid="706b871c-7692-11dc-8925-31e85bbc495c" />
                                </div>
                              </div>
                            </div>
                          </bodyText>
                          <documentRefs>
                            <OTUDLLeveledQuestions id="706b871c-7692-11dc-8925-31e85bbc495c" name="Scaffolded questions">
                              <questions>
                                <OTQuestion name="L5">
                                  <prompt>
                                    <OTUDLCompoundDoc>
                                      <bodyText>What is it like?</bodyText>
                                    </OTUDLCompoundDoc>
                                  </prompt>
                                  <input>
                                    <OTUDLCompoundDoc>
                                      <documentRefs>
                                        <OTText id="af298a3c-7692-11dc-8925-31e85bbc495c" name="Text field" />
                                      </documentRefs>
