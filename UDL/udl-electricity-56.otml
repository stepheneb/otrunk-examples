                                <OTUDLQuestion name="L5" showConvertButton="false" showScaffolding="true">
                                  <prompt>
                                    <OTCompoundDoc>
                                      <bodyText>What would happen if two negatively charged objects were brought together?</bodyText>
                                    </OTCompoundDoc>
                                  </prompt>
