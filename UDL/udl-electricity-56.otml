                            <OTDataCollector id="09187de2-a449-11dc-aee8-0d3d3ace7034" name="Temperature" title="Temperature Sensor Graph" autoScaleEnabled="true" multipleGraphableEnabled="true">
                              <source>
                                <OTDataGraphable name="two" connectPoints="true" yColumn="1" drawMarks="false" color="16711731" xColumn="0" lineWidth="2.0">
                                  <dataProducer>
                                    <OTSensorDataProxy name="dataproducer">
                                      <request>
                                        <OTExperimentRequest period="0.1">
                                          <sensorRequests>
                                            <OTSensorRequest stepSize="0.1" type="0" unit="degC" port="0" requiredMax="40.0" displayPrecision="-1" requiredMin="0.0" />
                                          </sensorRequests>
                                        </OTExperimentRequest>
                                      </request>
                                    </OTSensorDataProxy>
                                  </dataProducer>
                                  <dataStore>
                                    <OTDataStore dt="0.1" numberChannels="1">
                                      <channelDescriptions>
