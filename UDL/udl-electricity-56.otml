                                  </yDataAxis>
                                  <graphables>
                                    <OTDataGraphable drawMarks="false" color="65535" connectPoints="true" xColumn="0" yColumn="1" name="Second battery" lineWidth="2.0">
                                      <dataProducer>
                                        <OTSensorDataProxy>
                                          <request>
                                            <object refid="16780d50-cada-11dc-9f17-f581dffccb2a" />
                                          </request>
                                        </OTSensorDataProxy>
                                      </dataProducer>
                                      <dataStore>
                                        <OTDataStore dt="0.1" numberChannels="1">
                                          <channelDescriptions>
                                            <OTDataChannelDescription name="dt" precision="2" numericData="true" />
                                            <OTDataChannelDescription name="" unit="V" precision="-2" numericData="true" />
                                          </channelDescriptions>
                                        </OTDataStore>
                                      </dataStore>
                                    </OTDataGraphable>
                                    <OTDataGraphable drawMarks="false" color="8421504" connectPoints="true" xColumn="0" yColumn="1" name="Third battery" lineWidth="2.0">
                                      <dataProducer>
                                        <OTSensorDataProxy>
                                          <request>
                                            <object refid="16780d50-cada-11dc-9f17-f581dffccb2a" />
                                          </request>
                                        </OTSensorDataProxy>
                                      </dataProducer>
                                      <dataStore>
                                        <OTDataStore dt="0.1" numberChannels="1">
                                          <channelDescriptions>
                                            <OTDataChannelDescription name="dt" precision="2" numericData="true" />
                                            <OTDataChannelDescription name="" unit="V" precision="-2" numericData="true" />
                                          </channelDescriptions>
                                        </OTDataStore>
                                      </dataStore>
                                    </OTDataGraphable>
                                    <OTDataGraphable drawMarks="false" color="65280" connectPoints="true" xColumn="0" yColumn="1" name="Fourth battery" lineWidth="2.0">
                                      <dataProducer>
                                        <OTSensorDataProxy>
                                          <request>
                                            <object refid="16780d50-cada-11dc-9f17-f581dffccb2a" />
                                          </request>
                                        </OTSensorDataProxy>
                                      </dataProducer>
                                      <dataStore>
                                        <OTDataStore dt="0.1" numberChannels="1">
                                          <channelDescriptions>
                                            <OTDataChannelDescription name="dt" precision="2" numericData="true" />
                                            <OTDataChannelDescription name="" unit="V" precision="-2" numericData="true" />
                                          </channelDescriptions>
                                        </OTDataStore>
                                      </dataStore>
                                    </OTDataGraphable>
                                  </graphables>
                                </OTDataCollector>
                                <OTSnapshotButton id="c410dc27-cadb-11dc-9f17-f581dffccb2a" name="Snapshot button">
                                  <snapshotAlbum>
                                    <object refid="${album}" />
                                  </snapshotAlbum>
                                  <target>
                                    <object refid="0fcf57c5-cada-11dc-9f17-f581dffccb2a" />
                                  </target>
                                </OTSnapshotButton>
                              </documentRefs>
                            </OTUDLCompoundDoc>
                            <OTUDLCompoundDoc name="%8bd871d7-b967-11dc-9c96-c9838ca6fb43" showDefinitions="true">
                              <bodyText>
                                <div class="buffer">
                                  <div class="border">
                                    <div class="body">
                                      <table width="100%">
                                        <tr>
                                          <td>
                                            <div class="subtitle" />
                                          </td>
                                          <td align="right">
                                            <div class="page-number">5</div>
                                          </td>
                                        </tr>
                                      </table>
                                      Record your voltages below. Record them to the hundredth of a volt 
          (for example, 2.63 V).
                                      <br />
                                      <object refid="ec7cb233-b966-11dc-9c96-c9838ca6fb43" />
                                      <p>
                                        Predict the total voltage if you put batteries #1 and #2 together, 
            oriented the same way.
                                        <br />
                                        <br />
                                        <object refid="b8923922-fc3c-11dc-a738-1f42f753985b" />
                                        <br />
                                        <br />
                                        <object refid="dc2a0256-fc3c-11dc-a738-1f42f753985b" />
                                        <br />
                                      </p>
                                      Predict the total voltage if you line them up together, all oriented 
          the same way, with the + to the right.
                                      <br />
                                      <br />
                                      <object refid="f0ee79f7-fc3c-11dc-a738-1f42f753985b" />
                                      <br />
                                      <object refid="0ee70dab-fc3d-11dc-a738-1f42f753985b" />
                                      <br />
                                    </div>
                                  </div>
                                </div>
                              </bodyText>
                              <documentRefs>
                                <OTDataTable id="ec7cb233-b966-11dc-9c96-c9838ca6fb43" name="Data table" showLastRowForAddingNew="true">
                                  <dataStore>
                                    <OTDataStore numberChannels="2">
                                      <channelDescriptions>
                                        <OTDataChannelDescription name="battery" numericData="false" />
                                        <OTDataChannelDescription name="voltage" numericData="false" />
                                      </channelDescriptions>
                                      <values>
                                        <string># 1</string>
                                        <null />
                                        <string># 2</string>
                                        <null />
                                        <string># 3</string>
                                        <null />
                                        <string># 4</string>
                                      </values>
                                    </OTDataStore>
                                  </dataStore>
                                </OTDataTable>
                                <OTDataTable id="b8923922-fc3c-11dc-a738-1f42f753985b" name="Data table" showLastRowForAddingNew="true">
                                  <dataStore>
                                    <OTDataStore numberChannels="2">
                                      <channelDescriptions>
                                        <OTDataChannelDescription name="battery" numericData="false" />
                                        <OTDataChannelDescription name="voltage" numericData="false" />
                                      </channelDescriptions>
                                      <values>
                                        <string>#1</string>
                                        <null />
                                        <string>#2</string>
                                        <null />
                                        <string>combined</string>
                                      </values>
                                    </OTDataStore>
                                  </dataStore>
                                </OTDataTable>
                                <OTSnapshotButton id="dc2a0256-fc3c-11dc-a738-1f42f753985b" name="Snapshot button">
                                  <target>
                                    <object refid="b8923922-fc3c-11dc-a738-1f42f753985b" />
                                  </target>
                                  <snapshotAlbum>
                                    <object refid="${album}" />
                                  </snapshotAlbum>
                                </OTSnapshotButton>
                                <OTDataTable id="f0ee79f7-fc3c-11dc-a738-1f42f753985b" name="Data table" showLastRowForAddingNew="true">
                                  <dataStore>
                                    <OTDataStore numberChannels="2">
                                      <channelDescriptions>
                                        <OTDataChannelDescription name="battery" numericData="false" />
                                        <OTDataChannelDescription name="voltage" numericData="false" />
                                      </channelDescriptions>
