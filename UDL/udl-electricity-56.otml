                                      <input>
                                        <OTText />
                                      </input>
                                      <correctAnswer>
                                        <OTCompoundDoc>
                                          <bodyText>charged</bodyText>
                                        </OTCompoundDoc>
                                      </correctAnswer>
                                    </OTUDLQuestion>
                                  </documentRefs>
                                  <bodyText>
                                    <object refid="da5b3aab-c846-11dc-b8b6-397a07a82a77" />
                                  </bodyText>
                                </OTUDLCompoundDoc>
                                <OTUDLCompoundDoc id="5ab3043f-649c-11dd-a494-4fdf7e1f20f7" name="%5ab3043f-649c-11dd-a494-4fdf7e1f20f7" showEditBar="true">
                                  <documentRefs>
                                    <OTUDLQuestion id="f5e650d6-c846-11dc-b8b6-397a07a82a77" name="Question" showConvertButton="true">
                                      <prompt>
                                        <OTUDLCompoundDoc>
                                          <bodyText>Why do you think so?</bodyText>
                                        </OTUDLCompoundDoc>
                                      </prompt>
                                      <input>
                                        <OTText />
                                      </input>
                                      <correctAnswer>
                                        <OTCompoundDoc>
                                          <bodyText>It has more of one charge than the other.</bodyText>
