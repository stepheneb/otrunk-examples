                                      <prompt>
                                        <OTCompoundDoc>
                                          <bodyText>Increase the collision-chance. How does the motion of the electrons change?</bodyText>
                                        </OTCompoundDoc>
                                      </prompt>
                                      <input>
                                        <OTText />
                                      </input>
                                    </OTUDLQuestion>
