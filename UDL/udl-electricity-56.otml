                <OTUDLSection local_id="section_8" name="Sum volts" isComputerBased="false" isIntroduction="false" isMath="true" isPosttest="false" isPretest="false" isReading="false" isWrapup="false">
                  <content>
                    <OTCardContainer id="5a27e0ae-7764-11dc-bcdb-d9cc05265d45" contentsMayScroll="true" loadViewOnlyOnDemand="true">
                      <cards>
                        <OTUDLCompoundDoc id="5a27e0ad-7764-11dc-bcdb-d9cc05265d45" name="%5a27e0ad-7764-11dc-bcdb-d9cc05265d45">
                          <bodyText>
                            <div class="buffer">
                              <div class="border">
                                <div class="body">
                                  <table width="100%">
                                    <tr>
                                      <td>
                                        <!-- title -->
                                      </td>
                                      <td align="right">
                                        <div class="page-number">1</div>
                                      </td>
                                    </tr>
                                  </table>
                                  <div class="subtitle">Discovery question</div>
                                  What is the voltage of combinations of batteries?
                                  <br />
                                  <br />
                                  <blockquote>
                                    <img src="http://udl.concord.org/artwork/elect_56/load_flashlight/el_56_load_flashlight.png" border="1" />
                                  </blockquote>
                                  <br />
                                  In this activity you will measure what happens when batteries are added together in a series.
                                </div>
                              </div>
                            </div>
                          </bodyText>
                        </OTUDLCompoundDoc>
                        <OTUDLCompoundDoc name="%1e880bdb-b965-11dc-9c96-c9838ca6fb43">
                          <bodyText>
                            <div class="buffer">
                              <div class="border">
                                <div class="body">
                                  <table width="100%">
                                    <tr>
                                      <td>
                                        <!-- title -->
                                      </td>
                                      <td align="right">
                                        <div class="page-number">2</div>
                                      </td>
                                    </tr>
                                  </table>
                                  <div class="subtitle">Engage</div>
                                  <object refid="686fe0dd-b965-11dc-9c96-c9838ca6fb43" />
                                  <p />
                                  <object refid="ac3ee29a-b965-11dc-9c96-c9838ca6fb43" />
                                  <p />
                                </div>
                              </div>
                            </div>
                          </bodyText>
                          <documentRefs>
                            <OTQuestion id="686fe0dd-b965-11dc-9c96-c9838ca6fb43" name="Question">
                              <prompt>
                                <OTUDLCompoundDoc>
                                  <bodyText>Some electrical devices use many batteries in a row. Why do you think they do this?</bodyText>
                                </OTUDLCompoundDoc>
                              </prompt>
                              <input>
                                <OTText />
                              </input>
                              <correctAnswer>
                                <OTUDLCompoundDoc>
                                  <bodyText>Many batteries together can produce a larger voltage. Also they last  longer because there is energy available from each battery.</bodyText>
                                </OTUDLCompoundDoc>
                              </correctAnswer>
                            </OTQuestion>
                            <OTQuestion id="ac3ee29a-b965-11dc-9c96-c9838ca6fb43" name="Question">
                              <prompt>
                                <OTUDLCompoundDoc>
                                  <bodyText>Suppose you made a row of 5 batteries, all pointed the same way, and each one was 2.0 volts. What do you think the total voltage would be?</bodyText>
                                </OTUDLCompoundDoc>
                              </prompt>
                              <input>
                                <OTText />
                              </input>
                              <correctAnswer>
                                <OTUDLCompoundDoc>
                                  <bodyText>10 Volts</bodyText>
                                </OTUDLCompoundDoc>
                              </correctAnswer>
                            </OTQuestion>
                          </documentRefs>
                        </OTUDLCompoundDoc>
                        <OTUDLCompoundDoc name="%fa2cd6ab-b965-11dc-9c96-c9838ca6fb43">
                          <bodyText>
                            <div class="buffer">
                              <div class="border">
                                <div class="body">
                                  <table width="100%">
                                    <tr>
                                      <td>
                                        <!-- title -->
                                      </td>
                                      <td align="right">
                                        <div class="page-number">3</div>
                                      </td>
                                    </tr>
                                  </table>
                                  <div class="subtitle">Materials</div>
                                  <ul>
                                    <li>Voltage sensor</li>
                                    <li>4 AA or AAA batteries, some of them old or not fully charged</li>
                                    <li>Masking tape</li>
                                  </ul>
                                </div>
                              </div>
                            </div>
                          </bodyText>
                        </OTUDLCompoundDoc>
                        <OTUDLCompoundDoc name="%52ddc308-b966-11dc-9c96-c9838ca6fb43">
                          <bodyText>
                            <div class="buffer">
