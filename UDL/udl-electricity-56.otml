                            <OTUDLQuestion id="87baccbf-e48d-11dc-87fe-a953fe7a8e37" name="Question" showConvertButton="true">
                              <prompt>
                                <OTUDLCompoundDoc>
                                  <bodyText>The bulbs are in parallel in circuit A and are in series in circuit B.</bodyText>
                                </OTUDLCompoundDoc>
                              </prompt>
                              <input>
