                                <OTUDLCompoundDoc id="1448c434-64c9-11dd-9e86-ffa627342c16">
                                  <bodyText>Place two +1 squares and one -1 square on your number line.</bodyText>
                                </OTUDLCompoundDoc>
                                <OTUDLCompoundDoc id="189b2e65-64c9-11dd-9e86-ffa627342c16" name="%189b2e65-64c9-11dd-9e86-ffa627342c16" showEditBar="true">
