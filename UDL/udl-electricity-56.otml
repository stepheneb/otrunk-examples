                            </OTUDLCoachStatement>
                            <OTUDLCoachStatement timesClicked="1">
                              <coach>
                                <object refid="${character_strategic}" />
                              </coach>
                              <statement>
                                <OTUDLCompoundDoc>
                                  <bodyText>
                                    <div class="buffer">
                                      <div class="border">
                                        <div class="body">You will use a computer model to focus on the different parts needed to make a bulb light up.</div>
                                      </div>
                                    </div>
                                    <img src="http://udl.concord.org/share/shared-images/clunk-anim.gif" height="150" width="200" />
                                  </bodyText>
                                </OTUDLCompoundDoc>
                              </statement>
                            </OTUDLCoachStatement>
                            <OTUDLCoachStatement timesClicked="1">
                              <coach>
                                <object refid="${character_recognition}" />
                              </coach>
                              <statement>
                                <OTUDLCompoundDoc>
                                  <bodyText>
                                    <div class="buffer">
                                      <div class="border">
                                        <div class="body">By using a model of electrical equipment, you can experiment safely.  Think about each part and the way you connect them.</div>
                                      </div>
                                    </div>
                                    <img src="http://udl.concord.org/share/shared-images/roxy-anim.gif" height="150" width="200" />
                                  </bodyText>
                                </OTUDLCompoundDoc>
