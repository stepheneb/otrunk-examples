                                </OTUDLCompoundDoc>
                              </prompt>
                              <input>
                                <OTChoice name="Multiple choice" alignContent="0">
                                  <choices>
                                    <OTCompoundDoc name="red">
                                      <bodyText>red</bodyText>
                                    </OTCompoundDoc>
                                    <OTCompoundDoc name="orange">
                                      <bodyText>orange</bodyText>
                                    </OTCompoundDoc>
                                    <OTCompoundDoc name="white">
                                      <bodyText>white</bodyText>
                                    </OTCompoundDoc>
                                  </choices>
                                </OTChoice>
                              </input>
                            </OTQuestion>
