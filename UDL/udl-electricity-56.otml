                          <documentRefs>
                            <OTUDLQuestion id="f9920029-e494-11dc-94c7-8db1430529da" name="Question" showConvertButton="true">
                              <prompt>
                                <OTUDLCompoundDoc>
                                  <bodyText>5. The glowing wire inside a light bulb is called the filament. Which statements are true about what's going on inside the filament? (You can choose more than one.)</bodyText>
                                </OTUDLCompoundDoc>
                              </prompt>
                              <input>
                                <OTChoiceMultiple name="Multiple choice (check boxes)" alignContent="0">
                                  <choices>
                                    <OTCompoundDoc name="The filament makes light but no heat.">
                                      <bodyText>The filament makes light but no heat.</bodyText>
                                    </OTCompoundDoc>
                                    <OTCompoundDoc name="Moving charges hit atoms and heat them up.">
                                      <bodyText>Moving charges hit atoms and heat them up.</bodyText>
                                    </OTCompoundDoc>
                                    <OTCompoundDoc name="The filament makes both light and heat.">
                                      <bodyText>The filament makes both light and heat.</bodyText>
                                    </OTCompoundDoc>
                                    <OTCompoundDoc name="Electric sparks fly out of the filament and light up the room.">
