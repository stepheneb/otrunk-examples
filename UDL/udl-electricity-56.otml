                  <header>
                    <OTUDLCompoundDoc id="540fd1a9-7764-11dc-bcdb-d9cc05265d45" showEditBar="false" name="%540fd1a9-7764-11dc-bcdb-d9cc05265d45">
                      <bodyText>
                        <table width="100%">
                          <tr>
                            <td style="text-align: left">
                              <div class="title">Electrical adventure</div>
                            </td>
                            <td width="50px">
                              <object refid="16c37c27-7784-4c64-b8d0-ba3752d676ce" viewid="8e15a570-16a7-4fad-8775-778cb385bb8a" />
                            </td>
                          </tr>
                        </table>
                      </bodyText>
                      <documentRefs>
                        <object refid="16c37c27-7784-4c64-b8d0-ba3752d676ce" />
                        <object refid="8e15a570-16a7-4fad-8775-778cb385bb8a" />
                      </documentRefs>
