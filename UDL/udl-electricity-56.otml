                            <OTQuestion id="ad84ca16-b982-11dc-b953-933a44eb0f8e" name="Question">
                              <prompt>
                                <OTUDLCompoundDoc>
                                  <bodyText>How does cooling-rate affect atom temperature?</bodyText>
                                </OTUDLCompoundDoc>
                              </prompt>
                              <input>
                                <OTChoice name="Multiple choice" alignContent="0">
                                  <choices>
                                    <OTCompoundDoc name="no effect">
                                      <bodyText>no effect</bodyText>
                                    </OTCompoundDoc>
                                    <OTCompoundDoc name="greater cooling-rate lowers atom temperature">
                                      <bodyText>greater cooling-rate lowers atom temperature</bodyText>
                                    </OTCompoundDoc>
                                    <OTCompoundDoc name="greater cooling-rate raises atom temperature">
                                      <bodyText>greater cooling-rate raises atom temperature</bodyText>
                                    </OTCompoundDoc>
                                  </choices>
                                </OTChoice>
                              </input>
                              <correctAnswer>
                                <OTUDLCompoundDoc>
                                  <bodyText>greater cooling-rate lowers atom temperature</bodyText>
                                </OTUDLCompoundDoc>
                              </correctAnswer>
                            </OTQuestion>
