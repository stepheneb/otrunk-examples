                              <statement>
                                <OTUDLCompoundDoc>
                                  <bodyText>
                                    <div class="buffer">
                                      <div class="border">
                                        <div class="body">I predict the story will be about a birthday party and electrical 
          charges.</div>
                                      </div>
                                    </div>
                                    <img height="150" src="jres:///org/concord/otrunk/udl/document/roxy-anim.gif" width="200"/>
                                  </bodyText>
                                </OTUDLCompoundDoc>
                              </statement>
