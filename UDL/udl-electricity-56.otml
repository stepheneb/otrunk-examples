                            <OTUDLQuestion id="d049ab41-e48e-11dc-87fe-a953fe7a8e37" name="Question" showConvertButton="true">
                              <prompt>
                                <OTUDLCompoundDoc>
                                  <bodyText>3. Electrical energy is used to power a lamp. How does the amount of light energy coming from the lamp compare to the amount of electrical energy going to the lamp? The amount of light energy is:</bodyText>
                                </OTUDLCompoundDoc>
                              </prompt>
                              <input>
                                <OTChoice name="Multiple choice" alignContent="0">
                                  <choices>
                                    <OTCompoundDoc name="more than the amount of electrical energy.">
                                      <bodyText>more than the amount of electrical energy.</bodyText>
                                    </OTCompoundDoc>
                                    <OTCompoundDoc id="d049ab41-e48e-11dc-87fe-a953fe7a8e37/input/choices[1]" name="less than the amount of electrical energy.">
                                      <bodyText>less than the amount of electrical energy.</bodyText>
                                    </OTCompoundDoc>
                                    <OTCompoundDoc name="the same as the amount of electrical energy.">
