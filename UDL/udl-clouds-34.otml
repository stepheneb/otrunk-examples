                        <OTUDLCompoundDoc name="%16d17ab2-00c4-11dd-ab8f-d5c2d9ab838f">
                          <documentRefs>
                            <object refid="5ead6d0f-e653-11dc-a279-db0c302a32f2" />
                          </documentRefs>
                          <bodyText>
                            <div class="buffer">
