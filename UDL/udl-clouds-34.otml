                              <prompt>
                                <OTUDLCompoundDoc>
                                  <bodyText>Describe how the plastic wrap over the bowl of hot water looks different from the plastic wrap over the bowl of cold water.</bodyText>
                                </OTUDLCompoundDoc>
                              </prompt>
                              <input>
                                <OTText />
                              </input>
                            </OTUDLQuestion>
