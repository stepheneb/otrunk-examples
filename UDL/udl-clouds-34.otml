                              <statement>
                                <OTUDLCompoundDoc>
                                  <bodyText>
                                    <div class="buffer">
                                      <div class="border">
                                        <div class="body">I predict the children will grow wings to fly to the clouds.</div>
                                      </div>
                                    </div>
                                    <img src="jres:///org/concord/otrunk/udl/document/roxy-anim.gif" height="150" width="200"/>
