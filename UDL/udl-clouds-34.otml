                                </OTCompoundDoc>
                                <OTCompoundDoc name="Cotton.">
                                  <bodyText>Cotton.</bodyText>
                                </OTCompoundDoc>
                                <OTCompoundDoc name="All of the above are possible.">
                                  <bodyText>All of the above are possible.</bodyText>
                                </OTCompoundDoc>
                              </choices>
                            </OTChoice>
