                                    <tr>
                                      <td>
                                        <div class="subtitle" />
                                      </td>
                                      <td align="right">
                                        <div class="page-number">5</div>
                                      </td>
                                    </tr>
                                  </table>
                                  <div class="subtitle" />
                                  2. Squeeze the bottle hard and then release.
                                  <br />
                                  <br />
                                  <img border="1" src="http://udl.concord.org/artwork/cloud_34/bottle_squeezed/cl_34_bottle_squeezed.jpg" />
                                  <br />
                                  <br />
                                  <object refid="e12af287-f6d4-11dc-bfbc-812dc9708ee7" />
                                </div>
                              </div>
                            </div>
                          </bodyText>
                          <documentRefs>
                            <OTUDLLeveledQuestions id="e12af287-f6d4-11dc-bfbc-812dc9708ee7" name="Leveled questions">
                              <questions>
                                <OTUDLQuestion name="L5" showConvertButton="false" showScaffolding="true">
                                  <prompt>
                                    <OTUDLCompoundDoc>
                                      <bodyText>What happens when you squeeze the bottle?</bodyText>
                                    </OTUDLCompoundDoc>
                                  </prompt>
                                  <input>
                                    <OTChoice name="Multiple choice" alignContent="0">
                                      <choices>
                                        <OTCompoundDoc name="A cloud forms.">
                                          <bodyText>A cloud forms.</bodyText>
                                        </OTCompoundDoc>
                                        <OTCompoundDoc name="The cloud disappears.">
                                          <bodyText>The cloud disappears.</bodyText>
                                        </OTCompoundDoc>
                                        <OTCompoundDoc name="Nothing happens.">
                                          <bodyText>Nothing happens.</bodyText>
                                        </OTCompoundDoc>
                                      </choices>
                                    </OTChoice>
