                                      <input>
                                        <OTText text=""/>
                                      </input>
                                      <correctAnswer>
                                        <OTCompoundDoc>
                                          <bodyText>It looks like ice or snow. It is rigid, with a fixed shape. It is cold</bodyText>
                                        </OTCompoundDoc>
                                      </correctAnswer>
                                    </OTUDLQuestion>
