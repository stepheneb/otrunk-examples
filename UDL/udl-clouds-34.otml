                                    </OTCompoundDoc>
                                    <OTCompoundDoc name="lower humidity">
                                      <bodyText>lower humidity</bodyText>
                                    </OTCompoundDoc>
                                    <OTCompoundDoc name="unchanged">
                                      <bodyText>unchanged</bodyText>
                                    </OTCompoundDoc>
                                  </choices>
                                </OTChoice>
