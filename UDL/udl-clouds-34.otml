                <OTUDLSection local_id="section_8" name="Sunny or cloudy?" isComputerBased="false" isIntroduction="false" isMath="true" isPosttest="false" isPretest="false" isReading="false" isWrapup="false">
                  <content>
                    <OTCardContainer id="f63370dd-71c3-11dc-9410-0f9c34fad6c2" contentsMayScroll="true" loadViewOnlyOnDemand="true">
                      <cards>
                        <OTUDLCompoundDoc id="f63370dc-71c3-11dc-9410-0f9c34fad6c2" name="f63370dc-71c3-11dc-9410-0f9c34fad6c2">
                          <bodyText>
                            <div class="buffer">
