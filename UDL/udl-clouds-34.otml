                <OTUDLSection local_id="section_6" name="Create a Cloud" isComputerBased="true" isIntroduction="false" isMath="false" isPosttest="false" isPretest="false" isReading="false" isWrapup="false">
                  <content>
                    <OTCardContainer id="cb5292a8-71c3-11dc-9410-0f9c34fad6c2" contentsMayScroll="true" loadViewOnlyOnDemand="true">
                      <cards>
                        <OTUDLCompoundDoc id="cb5292a7-71c3-11dc-9410-0f9c34fad6c2" name="cb5292a7-71c3-11dc-9410-0f9c34fad6c2">
                          <bodyText>
                            <div class="buffer">
                              <div class="border">
                                <div class="body">
                                  <table width="100%">
                                    <tr>
                                      <td>
                                        <div class="subtitle">Discovery Question</div>
                                      </td>
                                      <td align="right">
                                        <div class="page-number">1</div>
                                      </td>
                                    </tr>
                                  </table>
                                  What are clouds made of?
                                  <p />
                                  <blockquote>
                                    <img border="1" src="http://udl.concord.org/artwork/cloud_34/beachtowel/beachtowel.jpg" />
                                  </blockquote>
                                  <br />
                                  In this activity you will create a cloud in a jar.
                                </div>
                              </div>
                            </div>
                          </bodyText>
                          <coachStatements>
                            <OTUDLCoachStatement>
                              <coach>
                                <object refid="${character_affective}" />
                              </coach>
                              <statement>
                                <OTUDLCompoundDoc>
                                  <bodyText>
                                    <div class="buffer">
                                      <div class="border">
                                        <div class="body">Have you played in the rain or the snow?  It can be wet or cold, but fun!</div>
                                      </div>
                                    </div>
                                    <img src="http://confluence.concord.org/download/attachments/12353/blah-head-small.png" height="150" width="200" />
                                  </bodyText>
                                </OTUDLCompoundDoc>
                              </statement>
                            </OTUDLCoachStatement>
                          </coachStatements>
                        </OTUDLCompoundDoc>
                        <OTUDLCompoundDoc name="%97795b2e-e65a-11dc-a279-db0c302a32f2">
                          <bodyText>
                            <div class="buffer">
                              <div class="border">
                                <div class="body">
                                  <table width="100%">
                                    <tr>
                                      <td>
                                        <div class="subtitle" />
                                      </td>
                                      <td align="right">
                                        <div class="page-number">2</div>
                                      </td>
                                    </tr>
                                  </table>
                                  <table width="100%">
                                    <tr>
                                      <td>
                                        <div class="subtitle">Engage</div>
                                      </td>
                                    </tr>
                                  </table>
                                  Look up the sky and on most days you will see clouds. Some of them 
          are big and puffy while others are thin and feathery. Have you ever 
          wondered what the clouds were made of?
                                  <br />
                                  <br />
                                  <img src="http://udl.concord.org/artwork/cloud_34/simplecloud/cl_34_simplecloud.jpg" border="1" />
                                  <br />
                                  <br />
                                  <object refid="1f49d08f-067e-11dd-8b2a-1fd30c4d997c" />
                                  <br />
                                </div>
                              </div>
                            </div>
                          </bodyText>
                          <documentRefs>
                            <OTUDLQuestion id="1f49d08f-067e-11dd-8b2a-1fd30c4d997c" name="Question" showConvertButton="true">
                              <prompt>
                                <OTCompoundDoc>
                                  <bodyText>What do you think clouds are made of?</bodyText>
                                </OTCompoundDoc>
                              </prompt>
                              <input>
                                <OTText />
                              </input>
                            </OTUDLQuestion>
                          </documentRefs>
                        </OTUDLCompoundDoc>
                        <OTUDLCompoundDoc name="6e60ff0b-71d7-11dc-9410-0f9c34fad6c2">
                          <bodyText>
                            <div class="buffer">
                              <div class="border">
                                <div class="body">
                                  <table width="100%">
                                    <tr>
                                      <td>
                                        <div class="subtitle">Materials</div>
                                      </td>
                                      <td align="right">
                                        <div class="page-number">3</div>
                                      </td>
                                    </tr>
                                  </table>
                                  <ul>
                                    <li>
                                      2-liter clear plastic bottle with cap
                                      <br />
                                    </li>
                                    <li>
                                      warm water
                                      <br />
                                    </li>
                                    <li>
                                      matches
                                      <br />
                                      <br />
                                    </li>
                                  </ul>
                                  <div class="subtitle">
                                    <font color="red">Safety</font>
                                  </div>
                                  <font color="red">
                                    <b>Caution</b>
                                  </font>
                                  : Only the teacher should 
          light and hold the match over the bottle. Severe burns may result.
                                </div>
                              </div>
                            </div>
                          </bodyText>
                        </OTUDLCompoundDoc>
                        <OTUDLCompoundDoc name="8c0c842c-71d7-11dc-9410-0f9c34fad6c2">
                          <bodyText>
                            <div class="buffer">
                              <div class="border">
                                <div class="body">
                                  <table width="100%">
                                    <tr>
                                      <td>
                                        <div class="subtitle">Explore</div>
                                      </td>
                                      <td align="right">
                                        <div class="page-number">4</div>
                                      </td>
                                    </tr>
                                  </table>
                                  1. Fill the clear plastic bottle 1/3 full of warm water and place 
          the cap on tightly. Shake it so that any surface condensation is 
          gone.
                                  <br />
                                  <br />
                                  <img src="http://udl.concord.org/artwork/cloud_34/bottle_capped/cl_34_bottle_capped.jpg" border="1" />
                                  <p />
                                  <br />
                                </div>
                              </div>
                            </div>
                          </bodyText>
                        </OTUDLCompoundDoc>
                        <OTUDLCompoundDoc name="%61bc441c-f6d4-11dc-bfbc-812dc9708ee7">
                          <bodyText>
                            <div class="buffer">
                              <div class="border">
                                <div class="body">
                                  <table width="100%">
                                    <tr>
                                      <td>
                                        <div class="subtitle" />
                                      </td>
                                      <td align="right">
                                        <div class="page-number">5</div>
                                      </td>
                                    </tr>
                                  </table>
                                  2. Squeeze the bottle hard and then release.
                                  <br />
                                  <br />
                                  <img src="http://udl.concord.org/artwork/cloud_34/bottle_squeezed/cl_34_bottle_squeezed.jpg" border="1" />
                                  <br />
                                  <br />
                                  <object refid="9bf81501-0257-11dd-96e5-3d1faeed689f" />
                                  <br />
                                </div>
                              </div>
                            </div>
                          </bodyText>
                          <documentRefs>
                            <OTUDLQuestion id="9bf81501-0257-11dd-96e5-3d1faeed689f" name="Question" showConvertButton="true">
                              <prompt>
                                <OTCompoundDoc>
                                  <bodyText>What happens when you squeeze the bottle?</bodyText>
                                </OTCompoundDoc>
                              </prompt>
                              <input>
                                <OTChoice name="Multiple choice" alignContent="0">
                                  <choices>
                                    <OTCompoundDoc name="A cloud forms.">
                                      <bodyText>A cloud forms.</bodyText>
                                    </OTCompoundDoc>
                                    <OTCompoundDoc name="The cloud disappears.">
                                      <bodyText>The cloud disappears.</bodyText>
                                    </OTCompoundDoc>
                                    <OTCompoundDoc name="Nothing happens.">
                                      <bodyText>Nothing happens.</bodyText>
                                    </OTCompoundDoc>
                                  </choices>
                                </OTChoice>
                              </input>
                            </OTUDLQuestion>
                          </documentRefs>
                        </OTUDLCompoundDoc>
                        <OTUDLCompoundDoc name="%4e1f8eb2-f6d5-11dc-bfbc-812dc9708ee7">
                          <bodyText>
                            <div class="buffer">
                              <div class="border">
                                <div class="body">
                                  <table width="100%">
                                    <tr>
                                      <td>
                                        <div class="subtitle" />
                                      </td>
                                      <td align="right">
                                        <div class="page-number">6</div>
                                      </td>
                                    </tr>
                                  </table>
                                  3.Take the cap off the bottle when the teacher is near you.
                                  <br />
                                  <br />
                                  4. 
          The teacher will light a match and hold it over the bottle for a few 
          seconds and drop the match in the bottle.
                                  <br />
                                  <br />
                                  5. Quickly cover 
          the bottle with the cap.
                                  <br />
                                  <br />
                                  <img src="http://udl.concord.org/artwork/cloud_34/bottle_match_lit/cl_34_bottle_match_lit.jpg" border="1" />
                                </div>
                              </div>
                            </div>
                          </bodyText>
                        </OTUDLCompoundDoc>
                        <OTUDLCompoundDoc name="%b2b73494-f6d5-11dc-bfbc-812dc9708ee7">
                          <documentRefs>
                            <OTUDLQuestion id="1c80dba0-0257-11dd-96e5-3d1faeed689f" name="Question" showConvertButton="true">
                              <prompt>
                                <OTCompoundDoc>
                                  <bodyText>8. What happens when you squeeze and then release the bottle?</bodyText>
                                </OTCompoundDoc>
                              </prompt>
                              <input>
                                <OTChoice name="Multiple choice" alignContent="0">
                                  <choices>
                                    <OTCompoundDoc name="A cloud forms.">
                                      <bodyText>A cloud forms.</bodyText>
                                    </OTCompoundDoc>
                                    <OTCompoundDoc name="The cloud disappears.">
                                      <bodyText>The cloud disappears.</bodyText>
                                    </OTCompoundDoc>
                                    <OTCompoundDoc name="Nothing happens.">
                                      <bodyText>Nothing happens.</bodyText>
                                    </OTCompoundDoc>
                                  </choices>
                                </OTChoice>
                              </input>
                            </OTUDLQuestion>
                          </documentRefs>
                          <bodyText>
                            <div class="buffer">
                              <div class="border">
                                <div class="body">
                                  <table width="100%">
                                    <tr>
                                      <td>
                                        <div class="subtitle" />
                                      </td>
                                      <td align="right">
                                        <div class="page-number">7</div>
                                      </td>
                                    </tr>
                                  </table>
                                  6. Shake the bottle again to get rid of water droplets.
                                  <br />
                                  <br />
                                  7. 
          Once again slowly squeeze the bottle hard and then release.
                                  <br />
                                  <br />
                                  <br />
                                  <object refid="1c80dba0-0257-11dd-96e5-3d1faeed689f" />
                                  <br />
                                </div>
                              </div>
                            </div>
                          </bodyText>
                        </OTUDLCompoundDoc>
                        <OTUDLCompoundDoc name="871048a0-71db-11dc-9410-0f9c34fad6c2">
                          <bodyText>
                            <div class="buffer">
                              <div class="border">
                                <div class="body">
                                  <table width="100%">
                                    <tr>
                                      <td>
                                        <div class="subtitle">Explain</div>
                                      </td>
                                      <td align="right">
                                        <div class="page-number">8</div>
                                      </td>
                                    </tr>
                                  </table>
                                  <p>
                                    <br />
                                    <object refid="b0932825-0256-11dd-96e5-3d1faeed689f" />
                                    <br />
                                    <br />
                                    <br />
                                    <object refid="d14313cb-0256-11dd-96e5-3d1faeed689f" />
                                  </p>
                                  <p>
                                    <br />
                                  </p>
                                </div>
                              </div>
                            </div>
                          </bodyText>
                          <documentRefs>
                            <OTUDLQuestion id="b0932825-0256-11dd-96e5-3d1faeed689f" name="Question" showConvertButton="true">
                              <prompt>
                                <OTCompoundDoc>
                                  <bodyText>Where do you think the cloud came from?</bodyText>
                                </OTCompoundDoc>
                              </prompt>
                              <input>
                                <OTText />
                              </input>
                            </OTUDLQuestion>
                            <OTUDLQuestion id="d14313cb-0256-11dd-96e5-3d1faeed689f" name="Question" showConvertButton="true">
                              <prompt>
                                <OTCompoundDoc>
                                  <bodyText>How do you know the clouds are not just the smoke form the matches?</bodyText>
                                </OTCompoundDoc>
                              </prompt>
                              <input>
                                <OTText />
                              </input>
                            </OTUDLQuestion>
                          </documentRefs>
                        </OTUDLCompoundDoc>
                        <OTUDLCompoundDoc name="%e2f9fd53-0256-11dd-96e5-3d1faeed689f">
                          <documentRefs>
                            <OTUDLQuestion id="eabe0eaa-0256-11dd-96e5-3d1faeed689f" name="Question" showConvertButton="true">
                              <prompt>
                                <OTCompoundDoc>
                                  <bodyText>What are clouds made of?</bodyText>
                                </OTCompoundDoc>
                              </prompt>
                              <input>
                                <OTText />
                              </input>
                            </OTUDLQuestion>
                          </documentRefs>
                          <bodyText>
                            <div class="buffer">
                              <div class="border">
                                <div class="body">
                                  <table width="100%">
                                    <tr>
                                      <td>
                                        <div class="subtitle" />
                                      </td>
                                      <td align="right">
                                        <div class="page-number">9</div>
                                      </td>
                                    </tr>
                                  </table>
                                  <div class="subtitle">
                                    <br />
                                    <object refid="eabe0eaa-0256-11dd-96e5-3d1faeed689f" />
                                  </div>
                                </div>
                              </div>
                            </div>
                          </bodyText>
                        </OTUDLCompoundDoc>
                        <OTUDLCompoundDoc name="%6087233b-f132-11dc-972d-1fafa9b56245">
                          <bodyText>
                            <div class="buffer">
                              <div class="border">
                                <div class="body">
                                  <table width="100%">
                                    <tr>
                                      <td>
                                        <div class="subtitle">Elaborate</div>
                                      </td>
                                      <td align="right">
                                        <div class="page-number">10</div>
                                      </td>
                                    </tr>
                                  </table>
                                  Just like a baker uses a recipe to bake a cake, the atmosphere must 
          have three ingredients to make a cloud. They are:
                                  <br />
                                   moisture
                                  <br />
                                   
          water evaporating
                                  <br />
                                   particles
                                  <br />
                                  <br />
                                  Water vapor can be 
          made to form small cloud droplets by adding particles such as smoke 
          or dust. This provides a surface for the  water vapor to condense 
          into water droplets. By squeezing the bottle you caused the water to 
          cool and condense around the smoke particles creating a cloud.
                                  <br />
                                  <br />
                                </div>
                              </div>
                            </div>
                          </bodyText>
                        </OTUDLCompoundDoc>
                        <OTUDLCompoundDoc name="%a9a64719-0680-11dd-8b2a-1fd30c4d997c">
                          <bodyText>
                            <div class="buffer">
                              <div class="border">
                                <div class="body">
                                  <table width="100%">
                                    <tr>
                                      <td>
                                        <div class="subtitle" />
                                      </td>
                                      <td align="right">
                                        <div class="page-number">11</div>
                                      </td>
                                    </tr>
                                  </table>
                                  The cloud you saw in the bottle is what a cloud really looks like. 
          Fog is a cloud close to the ground. Look at the picture below, the 
          lighthouse is in a cloud.
                                  <br />
                                  <br />
                                  <object refid="0b953910-0bc3-11dd-b37d-530805b004a4" />
                                  <br />
                                  <br />
                                  <img border="1" src="http://udl.concord.org/artwork/cloud_34/lighthousefog/lighthousefog.jpg" />
                                  <br />
                                  <br />
                                  Look 
          at the picture above, it appears that the lighthouse is in a cloud. 
          Fog is a cloud close to the ground.
                                  <br />
                                  <br />
                                  <object refid="e4bfaaee-0bc2-11dd-b37d-530805b004a4" />
                                  <br />
                                </div>
                              </div>
                            </div>
                          </bodyText>
                          <documentRefs>
                            <OTUDLQuestion id="0b953910-0bc3-11dd-b37d-530805b004a4" name="Question" showConvertButton="true">
                              <prompt>
                                <OTCompoundDoc>
                                  <bodyText>Have you ever walked through a cloud?</bodyText>
                                </OTCompoundDoc>
                              </prompt>
                              <input>
                                <OTText text="" />
                              </input>
                            </OTUDLQuestion>
                            <OTUDLQuestion id="e4bfaaee-0bc2-11dd-b37d-530805b004a4" name="Question" showConvertButton="true">
                              <prompt>
                                <OTCompoundDoc>
                                  <bodyText>Have you ever walked through fog?</bodyText>
                                </OTCompoundDoc>
                              </prompt>
                              <input>
                                <OTText />
                              </input>
                            </OTUDLQuestion>
                          </documentRefs>
                        </OTUDLCompoundDoc>
                      </cards>
                      <currentCard>
                        <object refid="cb5292a7-71c3-11dc-9410-0f9c34fad6c2" />
                      </currentCard>
                    </OTCardContainer>
                  </content>
                  <header>
                    <OTUDLCompoundDoc id="cb5292a9-71c3-11dc-9410-0f9c34fad6c2" showEditBar="false" name="cb5292a9-71c3-11dc-9410-0f9c34fad6c2">
                      <bodyText>
                        <div class="title">What are clouds made of?</div>
                      </bodyText>
                    </OTUDLCompoundDoc>
                  </header>
                  <footer>
                    <OTUDLCompoundDoc id="cb5292aa-71c3-11dc-9410-0f9c34fad6c2" showEditBar="false" name="cb5292aa-71c3-11dc-9410-0f9c34fad6c2">
                      <documentRefs>
                        <OTUDLMenu id="cb54dc9c-71c3-11dc-9410-0f9c34fad6c2">
                          <cardContainer>
                            <object refid="cb5292a8-71c3-11dc-9410-0f9c34fad6c2" />
                          </cardContainer>
                          <menuRule>
                            <object refid="${page-rule}" />
                          </menuRule>
                        </OTUDLMenu>
                        <object refid="29dc5ef0-c453-11dc-95ff-0800200c9a66" />
                      </documentRefs>
                      <bodyText>
                        <div align="center">
                          <table>
                            <tr>
                              <td>
                                <object refid="cb54dc9c-71c3-11dc-9410-0f9c34fad6c2" viewid="29dc5ef0-c453-11dc-95ff-0800200c9a66" />
                              </td>
                            </tr>
