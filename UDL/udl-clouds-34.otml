                            <OTUDLQuestion id="036ff05e-1544-11dd-875d-07a82165daa1" name="Question" showConvertButton="true">
                              <prompt>
                                <OTCompoundDoc>
                                  <bodyText>Observe the outside surfaces of the warm cup and the cold cup. On which surface does water form?</bodyText>
                                </OTCompoundDoc>
                              </prompt>
                              <input>
                                <OTChoice name="Multiple choice" alignContent="0" horizontal="false">
                                  <choices>
                                    <OTCompoundDoc name="warm cup">
