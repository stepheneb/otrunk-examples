                              <documentRefs>
                                <OTCardContainer id="f2a6b49f-5f40-11dd-b853-57f2247cadaa">
                                  <cards>
                                    <OTUDLCompoundDoc id="f2a6b4a1-5f40-11dd-b853-57f2247cadaa">
                                      <bodyText>Water moves around the earth in what is known as the water cycle. As the 
    water moves, it takes different forms, such as rain, snow, clouds, and 
    ocean water.</bodyText>
                                    </OTUDLCompoundDoc>
                                    <OTUDLCompoundDoc id="fdcfe9f2-5f40-11dd-b853-57f2247cadaa" name="%fdcfe9f2-5f40-11dd-b853-57f2247cadaa" showEditBar="true">
                                      <bodyText>Water on earth is sometimes in the form of a solid, like snow and ice.</bodyText>
                                    </OTUDLCompoundDoc>
                                    <OTUDLCompoundDoc id="0a2c2103-5f41-11dd-b853-57f2247cadaa" name="%0a2c2103-5f41-11dd-b853-57f2247cadaa" showEditBar="true">
                                      <bodyText>
                                        <object refid="1342f11b-5f41-11dd-b853-57f2247cadaa" />
                                        <br />
                                      </bodyText>
                                      <documentRefs>
                                        <OTUDLQuestion id="1342f11b-5f41-11dd-b853-57f2247cadaa" name="Question" showConvertButton="true">
                                          <prompt>
                                            <OTCompoundDoc>
                                              <bodyText>Describe what water is like when it's a solid.</bodyText>
                                            </OTCompoundDoc>
                                          </prompt>
                                          <input>
                                            <OTText text="" />
                                          </input>
                                        </OTUDLQuestion>
                                      </documentRefs>
                                    </OTUDLCompoundDoc>
                                    <OTUDLCompoundDoc id="2c98b0b3-5f41-11dd-b853-57f2247cadaa" name="%2c98b0b3-5f41-11dd-b853-57f2247cadaa" showEditBar="true">
                                      <bodyText>
                                        Place a snowflake stamp on the places where water can be found as a solid.
                                        <br />
                                        <br />
                                        Take a snapshot of your drawing.
                                      </bodyText>
                                    </OTUDLCompoundDoc>
                                  </cards>
                                  <currentCard>
                                    <object refid="f2a6b4a1-5f40-11dd-b853-57f2247cadaa" />
                                  </currentCard>
                                </OTCardContainer>
                                <object refid="29dc5ef0-c453-11dc-95ff-0800200c9a66" />
                                <OTUDLMenu id="f2a6b4a0-5f40-11dd-b853-57f2247cadaa" showBorder="false">
                                  <cardContainer>
                                    <object refid="f2a6b49f-5f40-11dd-b853-57f2247cadaa" />
                                  </cardContainer>
                                </OTUDLMenu>
                                <OTDrawingTool2 id="5ead6d0f-e653-11dc-a279-db0c302a32f2" name="Draw tool" backgroundImage="http://udl.concord.org/artwork/cloud_34/cycle1/cl_34_cycle2.jpg" backgroundImageURL="http://udl.concord.org/artwork/cloud_34/cycle1/cl_34_cycle2.jpg" largeTextButtonVisible="false" lineWidthButtonVisible="true" usePopupButtons="false">
                                  <stamps>
