                    </OTCardContainer>
                  </content>
                  <header>
                    <OTUDLCompoundDoc id="6cebf86b-71c2-11dc-9410-0f9c34fad6c2" showEditBar="false" name="6cebf86b-71c2-11dc-9410-0f9c34fad6c2">
                      <bodyText>
                        <div class="title">Where on Earth does the water go?</div>
                      </bodyText>
                    </OTUDLCompoundDoc>
                  </header>
                  <footer>
                    <OTUDLCompoundDoc id="6cebf86c-71c2-11dc-9410-0f9c34fad6c2" showEditBar="false" name="6cebf86c-71c2-11dc-9410-0f9c34fad6c2">
                      <documentRefs>
                        <OTUDLMenu id="6ceeb78e-71c2-11dc-9410-0f9c34fad6c2">
                          <cardContainer>
                            <object refid="6cebf86a-71c2-11dc-9410-0f9c34fad6c2" />
                          </cardContainer>
                          <menuRule>
                            <object refid="${page-rule}" />
                          </menuRule>
                        </OTUDLMenu>
                        <object refid="29dc5ef0-c453-11dc-95ff-0800200c9a66" />
                      </documentRefs>
                      <bodyText>
                        <div align="center">
                          <table>
                            <tr>
                              <td>
                                <object refid="6ceeb78e-71c2-11dc-9410-0f9c34fad6c2" viewid="29dc5ef0-c453-11dc-95ff-0800200c9a66" />
                              </td>
