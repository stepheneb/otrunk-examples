                              <imageSrc>
                                <OTBlob src="http://er.concord.org/artwork/animals/foxwhiteback.jpg" name="http://er.concord.org/artwork/animals/foxwhiteback.jpg" />
                              </imageSrc>
                            </OTDrawingStamp>
                            <OTDrawingStamp>
                              <imageSrc>
                                <OTBlob src="http://er.concord.org/artwork/animals/bunnywhiteback.jpg" name="http://er.concord.org/artwork/animals/bunnywhiteback.jpg" />
                              </imageSrc>
                            </OTDrawingStamp>
                            <OTDrawingStamp>
                              <imageSrc>
                                <OTBlob src="http://er.concord.org/artwork/animals/plantstamp.jpg" name="http://er.concord.org/artwork/animals/plantstamp.jpg" />
                              </imageSrc>
                            </OTDrawingStamp>
                          </stamps>
                        </OTDrawingTool2>
