                      <documentRefs>
                        <OTQuestion id="be20efbc-e8f3-11de-b3ff-79ded19af925" name="Question">
                          <prompt>
                            <OTCompoundDoc>
                              <bodyText>1. Describe the life cycle of the Mystery Plants in the field.</bodyText>
                            </OTCompoundDoc>
                          </prompt>
                          <input>
                            <OTText />
                          </input>
                        </OTQuestion>
                        <OTQuestion id="d0026bb3-e8f3-11de-b3ff-79ded19af925" name="Question" showImmediateFeedback="true" showCheckAnswersButton="false" incorrectAnswerFeedback="That's not correct.">
                          <prompt>
                            <OTCompoundDoc>
                              <bodyText>2. What happens to the parent plant during the winter?</bodyText>
                            </OTCompoundDoc>
                          </prompt>
                          <input>
                            <OTChoice name="Multiple choice" horizontal="false" alignContent="0">
                              <choices>
                                <OTCompoundDoc name="The parent plant lives through the winter.">
                                  <bodyText>The parent plant lives through the winter.</bodyText>
                                </OTCompoundDoc>
                                <OTCompoundDoc name="The parent plant dies and it grows back in the spring.">
                                  <bodyText>The parent plant dies and it grows back in the spring.</bodyText>
                                </OTCompoundDoc>
                                <OTCompoundDoc id="d0026bb3-e8f3-11de-b3ff-79ded19af925/input/choices[2]" name="The parent plant dies but its offspring grow in the spring.">
                                  <bodyText>The parent plant dies but its offspring grow in the spring.</bodyText>
                                </OTCompoundDoc>
                              </choices>
                            </OTChoice>
                          </input>
                          <correctAnswer>
                            <OTObjectSet>
                              <objects>
                                <object refid="d0026bb3-e8f3-11de-b3ff-79ded19af925/input/choices[2]" />
                              </objects>
                            </OTObjectSet>
                          </correctAnswer>
                        </OTQuestion>
