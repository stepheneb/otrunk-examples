                            <OTJavascript src="scripts/sunlight-with-mutation/add_organisms.js" />
                          </script>
                          <scriptVariables>
                            <OTScriptVariableView name="envView">
                              <reference>
                                <object refid="${population_view}" />
                              </reference>
                            </OTScriptVariableView>
                            <OTScriptVariable name="mutationCheck">
                              <reference>
                                <object refid="${mutation-check}" />
                              </reference>
                            </OTScriptVariable>
                          </scriptVariables>
                        </OTScriptButton>
                        <OTScriptButton local_id="run-button" text="Run">
                          <script>
                            <OTJavascript src="scripts/sunlight-with-mutation/run.js" />
                          </script>
                          <scriptVariables>
                            <OTScriptVariableView name="envView">
