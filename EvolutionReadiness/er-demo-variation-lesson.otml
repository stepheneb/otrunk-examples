                                <object refid="${population_view}" />
                              </reference>
                            </OTScriptVariableView>
                          </scriptVariables>
                        </OTScriptButton>
                        <OTScriptButton local_id="show-props-button" text="Show property">
                          <script>
                            <OTJavascript src="scripts/sunlight-with-mutation/show_property.js" />
                          </script>
