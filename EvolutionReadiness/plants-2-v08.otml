                        </OTScriptObject>
                        <OTLabbookButton id="1ec43c1e-8c2c-11de-acb1-dd44929be929" name="Labbook button">
                          <target>
                            <object refid="13d3fb4f-123a-425f-9d60-d84e145c0678!/env-holder" />
