          <OTCompoundDoc>
            <bodyText>Plantas de Hoja Tamaño 1</bodyText>
          </OTCompoundDoc>
        </entry>
        <entry key="816031b6-a228-11de-ba91-733ae0921aa5/input">
          <OTChoice>
            <choices>
              <object refid="9e59f84f-1b3f-11df-b18b-2d3fc761c535" />
