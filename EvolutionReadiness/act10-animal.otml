                                </OTCompoundDoc>
                                <OTCompoundDoc id="a6f96f40-e1e1-11df-b588-e382053b04c8" name="Light">
                                  <bodyText>Light</bodyText>
                                </OTCompoundDoc>
                                <OTCompoundDoc id="a6f96f41-e1e1-11df-b588-e382053b04c8" name="Water">
                                  <bodyText>Water</bodyText>
                                </OTCompoundDoc>
                                <OTCompoundDoc id="a564775f-e1e1-11df-b588-e382053b04c8" name="Light">
                                  <bodyText>Soil</bodyText>
                                </OTCompoundDoc>
                              </choices>
                            </OTChoice>
                          </input>
                        </OTQuestion>
                        <OTQuestion id="69cfa0b8-e1e1-11df-b588-e382053b04c8" name="Question">
                          <prompt>
                            <OTCompoundDoc>
                              <bodyText>2. What do rabbits rabbits eat?</bodyText>
                            </OTCompoundDoc>
                          </prompt>
                          <input>
                            <OTChoice name="Multiple choice" horizontal="false" alignContent="0">
                              <choices>
                                <OTCompoundDoc id="939b8c78-e1e1-11df-b588-e382053b04c8" name="Hawks">
                                  <bodyText>Hawks</bodyText>
                                </OTCompoundDoc>
                                <OTCompoundDoc id="939b8c79-e1e1-11df-b588-e382053b04c8" name="Foxes">
                                  <bodyText>Foxes</bodyText>
                                </OTCompoundDoc>
                                <OTCompoundDoc id="7f0354f6-e1e1-11df-b588-e382053b04c8" name="Hawks">
