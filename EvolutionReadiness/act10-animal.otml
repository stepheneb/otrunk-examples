                      <documentRefs>
                        <OTQuestion id="b011eebc-9b23-11df-8184-6f2452a94eae" name="Question" showImmediateFeedback="false" showCheckAnswersButton="false">
                          <prompt>
                            <OTCompoundDoc>
                              <bodyText>3. What would happen to the white and brown rabbits in the population if there were no hawks??</bodyText>
                            </OTCompoundDoc>
                          </prompt>
                          <input>
                            <OTChoice name="Multiple choice" horizontal="false" alignContent="0">
                              <choices>
                                <OTCompoundDoc id="46d9f4c0-b077-11df-a960-17daa6f2af96" name="There would be more brown rabbits in the population as the field changed to brown.">
                                  <bodyText>There would be more brown rabbits in the population as the field changed to brown.</bodyText>
                                </OTCompoundDoc>
                                <OTCompoundDoc id="3c4202ef-b077-11df-a960-17daa6f2af96" name="There would be more brown rabbits in the population as the field changed to brown.">
                                  <bodyText>There would be more white rabbits in the population as the field turned to brown.</bodyText>
                                </OTCompoundDoc>
                                <OTCompoundDoc id="64048a1a-b077-11df-a960-17daa6f2af96" name="The population of brown and white rabbits would remain about the same as the field changed to brown.">
                                  <bodyText>The population of brown and white rabbits would remain about the same as the field changed to brown.</bodyText>
                                </OTCompoundDoc>
                              </choices>
                            </OTChoice>
                          </input>
                        </OTQuestion>
                      </documentRefs>
