                        <OTQuestion id="b6c3a3fa-e1e1-11df-b588-e382053b04c8" name="Question">
                          <prompt>
                            <OTCompoundDoc>
                              <bodyText>3. What do hawks eat?</bodyText>
                            </OTCompoundDoc>
                          </prompt>
                          <input>
                            <OTChoice name="Multiple choice" horizontal="false" alignContent="0">
                              <choices>
                                <OTCompoundDoc id="cb0026f9-e1e1-11df-b588-e382053b04c8" name="Hawks">
                                  <bodyText>Hawks</bodyText>
                                </OTCompoundDoc>
                                <OTCompoundDoc id="cb0026fa-e1e1-11df-b588-e382053b04c8" name="Foxes">
                                  <bodyText>Foxes</bodyText>
                                </OTCompoundDoc>
                                <OTCompoundDoc id="cb004e0b-e1e1-11df-b588-e382053b04c8" name="Grass">
                                  <bodyText>Grass</bodyText>
                                </OTCompoundDoc>
                                <OTCompoundDoc id="cb004e0c-e1e1-11df-b588-e382053b04c8" name="All of the above">
                                  <bodyText>All of the above</bodyText>
                                </OTCompoundDoc>
                                <OTCompoundDoc id="c24c8587-e1e1-11df-b588-e382053b04c8" name="Hawks">
                                  <bodyText>Hawks</bodyText>
                                </OTCompoundDoc>
                                <OTCompoundDoc id="c24c8588-e1e1-11df-b588-e382053b04c8" name="Foxes">
                                  <bodyText>Foxes</bodyText>
                                </OTCompoundDoc>
                                <OTCompoundDoc id="c00b6f26-e1e1-11df-b588-e382053b04c8" name="Hawks">
                                  <bodyText>Grass</bodyText>
                                </OTCompoundDoc>
