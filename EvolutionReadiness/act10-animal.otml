                                  <bodyText>Light</bodyText>
                                </OTCompoundDoc>
                                <OTCompoundDoc id="ab585c93-e1e1-11df-b588-e382053b04c8" name="Water">
                                  <bodyText>Water</bodyText>
                                </OTCompoundDoc>
                                <OTCompoundDoc id="ab585c94-e1e1-11df-b588-e382053b04c8" name="Soil">
