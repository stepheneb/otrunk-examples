                                    <object refid="89cad2ae-23f1-4459-b03f-8748a878fe56!/script" />
                                  </td>
                                </tr>
                              </table>
                              <br />
                            </div>
                          </div>
                        </div>
                      </bodyText>
                      <documentRefs>
                        <OTQuestion id="96d77ea0-aa43-11df-81a7-ada6e115505a" name="Question">
                          <prompt>
                            <OTCompoundDoc>
                              <bodyText>6. Will the rabbit population in the bottom half of the field change or stay the same if the dam is removed?</bodyText>
                            </OTCompoundDoc>
                          </prompt>
                          <input>
                            <OTChoice name="Multiple choice" horizontal="false" alignContent="0">
                              <choices>
                                <OTCompoundDoc id="a137497d-aa43-11df-81a7-ada6e115505a" name="The population of rabbits will change.">
                                  <bodyText>The population of rabbits will change.</bodyText>
                                </OTCompoundDoc>
                                <OTCompoundDoc id="a137497e-aa43-11df-81a7-ada6e115505a" name="The population of rabbits will stay the same.">
                                  <bodyText>The population of rabbits will stay the same.</bodyText>
                                </OTCompoundDoc>
                                <OTCompoundDoc id="9d1da28c-aa43-11df-81a7-ada6e115505a" name="The population of rabbits will change.">
                                  <bodyText>The population of rabbits will change.</bodyText>
                                </OTCompoundDoc>
                              </choices>
