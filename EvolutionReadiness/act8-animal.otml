                                        <td>
                                          <table>
                                            <tr>
                                              <td height="40%">
                                                <object renderheight="200" renderwidth="400" refid="b37dcbd7-34ad-4187-bff0-f3a42be0b282!/bar-graph-top" />
                                              </td>
