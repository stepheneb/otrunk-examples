                        <OTQuestion id="35598590-8abf-11df-9a20-9121543cae86" name="Question" showImmediateFeedback="true" showCheckAnswersButton="false">
                          <prompt>
                            <OTCompoundDoc>
                              <bodyText>9. What do you think happened to the peppered moth population when the tree bark turned white?</bodyText>
                            </OTCompoundDoc>
                          </prompt>
                          <input>
                            <OTChoice name="Multiple choice" horizontal="false" alignContent="0">
                              <choices>
                                <OTCompoundDoc id="6b7f0fb5-8abf-11df-9a20-9121543cae86" name="The population of moths stayed black and were all eaten up!">
                                  <bodyText>The population of moths stayed black and were all eaten up!</bodyText>
                                </OTCompoundDoc>
                                <OTCompoundDoc id="6b7f0fb6-8abf-11df-9a20-9121543cae86" name="The population of moths slowly changed back to white.">
                                  <bodyText>The population of moths slowly shifted back to white.</bodyText>
                                </OTCompoundDoc>
                                <OTCompoundDoc id="6788cf44-8abf-11df-9a20-9121543cae86" name="The population of moths stayed black and were all eaten up!">
                                  <bodyText>The population of moths moved to another country.</bodyText>
                                </OTCompoundDoc>
                              </choices>
                            </OTChoice>
                          </input>
                          <correctAnswer>
                            <OTObjectSet>
                              <objects>
                                <object refid="6b7f0fb6-8abf-11df-9a20-9121543cae86" />
                              </objects>
                            </OTObjectSet>
                          </correctAnswer>
                        </OTQuestion>
                      </documentRefs>
