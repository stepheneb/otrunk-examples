                        <object refid="774f5f1d-5ddd-11df-98e6-fd7b52a8593e" />
                        <OTQuestion id="7f307b8a-5ea6-11df-8c93-93f7cffc0502" name="Question" showImmediateFeedback="true" showCheckAnswersButton="false" incorrectAnswerFeedback="That's not correct.  The plants reproduce fastest at Level 4.">
                          <prompt>
                            <OTCompoundDoc>
                              <bodyText>1. Which level of rainfall was best for all the plants?</bodyText>
                            </OTCompoundDoc>
                          </prompt>
                          <input>
                            <OTChoice name="Multiple choice" horizontal="false" alignContent="0">
                              <choices>
                                <OTCompoundDoc id="85987f57-5ea6-11df-8c93-93f7cffc0502" name="Level 5 (Most rain)">
