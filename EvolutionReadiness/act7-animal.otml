                              <objects>
                                <OTCompoundDoc id="85987f58-5ea6-11df-8c93-93f7cffc0502" name="Level 4">
                                  <bodyText>Level 4</bodyText>
                                </OTCompoundDoc>
                              </objects>
                            </OTObjectSet>
                          </correctAnswer>
                        </OTQuestion>
                        <OTQuestion id="03842c6a-8a98-11df-b985-d9ad3a9cfccc" name="Question" showImmediateFeedback="true" showCheckAnswersButton="false">
                          <prompt>
                            <OTCompoundDoc>
                              <bodyText>2. Were any of the plants able to survive when the rainfall was low?</bodyText>
                            </OTCompoundDoc>
                          </prompt>
                          <input>
                            <OTChoice name="Multiple choice" horizontal="false" alignContent="0">
                              <choices>
                                <OTCompoundDoc id="3038921e-8a98-11df-b985-d9ad3a9cfccc" name="Yes, Grass A was able to survive.">
                                  <bodyText>Yes, Grass A was able to survive.</bodyText>
                                </OTCompoundDoc>
                                <OTCompoundDoc id="3038921f-8a98-11df-b985-d9ad3a9cfccc" name="Yes, Grass B was able to survive.">
                                  <bodyText>Yes, Grass B was able to survive.</bodyText>
                                </OTCompoundDoc>
                                <OTCompoundDoc id="30389220-8a98-11df-b985-d9ad3a9cfccc" name="Yes, Grass C was able to survive.">
