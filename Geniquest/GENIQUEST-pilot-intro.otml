                            </OTBreedOffspring>
                            <OTBreedOffspring local_id="strain_breedoffspring">
                              <motherOrganism>
                                <OTOrganism local_id="strain_1_female_breedoffspring" name="Strain 1 Female" sex="1" allowFatalCharacteristic="false" alleles="b:h,a:H,b:S,a:S,b:W,a:w,b:L,a:l,b:t,a:t,b:P,a:P,b:F,a:f,b:a,a:a,b:B,a:B">
                                  <world>
                                    <OTWorld speciesPath="org/concord/biologica/worlds/dragonmarkers.xml" />
                                  </world>
                                </OTOrganism>
