                          <documentRefs>
                            <OTText id="fa8552fb-73b1-11dd-a769-cbc7ca7b3ef4" name="Text field!" text="Tanada and Landar also hope to be able to have a baby soon. This is a view of their cells as they undergo the process of meiosis. Click on the play button to start the process in the mother, Tanada." />
								            <OTOrganism id="a8e20bf5-73a4-11dd-a769-cbc7ca7b3ef4" name="Tanada" sex="1" allowFatalCharacteristic="false" alleles="b:h,a:H,b:S,a:S,b:W,a:w,b:L,a:L,b:t,a:t,b:p,a:p,b:F,a:f,b:a,a:a,b:B,a:B">
								              <world>
