                              </organism>
                            </OTStaticOrganism>
  
								            <OTOrganism id="b87576ac-651c-11dd-bd45-d9d11cbf97e7" name="Molthwan" sex="0" alleles="b:h,a:h,b:S,a:S,b:w,a:w,b:l,a:l,b:t,a:t,a:p,a:f,a:a,a:B">
