	                              </organisms>
	                              <listenTo>
	                                <object refid="7bb894fa-206a-11de-a205-9f7641662199" />
	                              </listenTo>
	                            </OTChromosomeZoom>
	                          </documentRefs>
	                        </OTCompoundDoc>
