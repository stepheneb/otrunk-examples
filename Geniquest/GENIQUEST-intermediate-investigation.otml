	                            <OTChromosomeZoom id="70ce7dbb-1f32-11de-a015-f7e7e09f8260" name="Chromosome Zoom" drawCrossover="true" organismLabelType="2" zoomLevel="0" chromosomePositionInBasePairs="0" chromosomePositionInCM="0.0" imageLabelNameTextVisible="false">
	                              <organisms>
	                                <object refid="${maleStrainOrganism}" />
