                          </bodyText>
                          <documentRefs>
                            <object refid="0949da57-1d8e-11de-8216-8348550e42a0" />
                            <OTQuestion id="b9e688b9-1e7a-11de-a8de-e53ab23ee3e2" name="Question" >
                              <prompt>
                                <OTCompoundDoc>
                                  <bodyText>Based on your breeding experiments, where do you think the gene for plates is located? (Hint: If you don't have a snapshot of the dragon chromosomes, you can use the chromosome tool to examine one of the dragons above.)</bodyText>
                                </OTCompoundDoc>
                              </prompt>
                              <input>
                                <OTChoice name="Multiple choice" alignContent="0">
                                  <choices>
                                    <OTCompoundDoc id="e8a95c0d-1e7a-11de-a8de-e53ab23ee3e2" name="Chromosome 1">
                                      <bodyText>Chromosome 1</bodyText>
                                    </OTCompoundDoc>
                                    <OTCompoundDoc name="Chromosome 2">
                                      <bodyText>Chromosome 2</bodyText>
                                    </OTCompoundDoc>
